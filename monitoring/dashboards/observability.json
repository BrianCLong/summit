{
  "id": null,
  "uid": "obs-overview",
  "title": "Observability Overview",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Resolver p95 (ms)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(graphql_resolver_duration_ms_bucket[5m])) by (le))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Neo4j Query ms",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(neo4j_query_duration_ms_bucket[5m])) by (le))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Cache Hit Ratio",
      "targets": [
        {
          "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m]))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    },
    {
      "type": "timeseries",
      "title": "LLM Call ms",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(llm_request_duration_ms_bucket[5m])) by (le))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
    },
    {
      "type": "timeseries",
      "title": "HTTP RTT ms",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_ms_bucket[5m])) by (le))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
    },
    {
      "type": "timeseries",
      "title": "Data Refresh Lag",
      "targets": [{ "expr": "data_refresh_lag_seconds" }],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
    }
  ]
}
