{
  "dashboard": {
    "id": null,
    "title": "GA Core Go/No-Go Dashboard",
    "tags": ["ga-core", "release", "metrics"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "üéØ GA Core Release Status",
        "type": "stat",
        "targets": [
          {
            "expr": "ga_core_overall_status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "basic"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "‚ùå NO GO",
                    "color": "red"
                  },
                  "1": {
                    "text": "‚úÖ GO",
                    "color": "green"
                  },
                  "0.5": {
                    "text": "‚ö†Ô∏è CONDITIONAL GO", 
                    "color": "yellow"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "value"
        }
      },
      {
        "id": 2,
        "title": "ü§ñ Entity Resolution Precision",
        "type": "stat",
        "targets": [
          {
            "expr": "er_precision_person_current",
            "refId": "A",
            "legendFormat": "PERSON"
          },
          {
            "expr": "er_precision_org_current", 
            "refId": "B",
            "legendFormat": "ORG"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "basic"
            },
            "min": 0.8,
            "max": 1.0,
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0.8
                },
                {
                  "color": "yellow", 
                  "value": 0.87
                },
                {
                  "color": "green",
                  "value": 0.9
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 4
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 3,
        "title": "üìä Merge Decision Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(merge_decision_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(merge_decision_duration_seconds_bucket[5m]))",
            "refId": "B", 
            "legendFormat": "p50"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 4
        },
        "yAxes": [
          {
            "unit": "s",
            "min": 0,
            "max": 10
          }
        ],
        "thresholds": [
          {
            "value": 5,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "title": "üõ°Ô∏è Policy Denials & Appeals",
        "type": "stat",
        "targets": [
          {
            "expr": "policy_denials_with_appeals_rate",
            "refId": "A",
            "legendFormat": "Appeals Available"
          },
          {
            "expr": "policy_appeal_sla_compliance_rate",
            "refId": "B",
            "legendFormat": "SLA Compliance"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0.8,
            "max": 1.0,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0.8
                },
                {
                  "color": "yellow",
                  "value": 0.9
                },
                {
                  "color": "green",
                  "value": 0.95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 5,
        "title": "üì¶ Export Manifest Integrity",
        "type": "stat",
        "targets": [
          {
            "expr": "export_manifest_integrity_rate",
            "refId": "A",
            "legendFormat": "Manifest Integrity"
          },
          {
            "expr": "export_bundle_verification_rate",
            "refId": "B",
            "legendFormat": "Bundle Verification"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0.9,
            "max": 1.0,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0.9
                },
                {
                  "color": "yellow",
                  "value": 0.95
                },
                {
                  "color": "green",
                  "value": 0.98
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "üìà ER Precision Trend (7 days)",
        "type": "graph",
        "targets": [
          {
            "expr": "er_precision_person_daily",
            "refId": "A",
            "legendFormat": "PERSON Precision"
          },
          {
            "expr": "er_precision_org_daily",
            "refId": "B",
            "legendFormat": "ORG Precision"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "yAxes": [
          {
            "min": 0.8,
            "max": 1.0,
            "unit": "percentunit"
          }
        ],
        "thresholds": [
          {
            "value": 0.9,
            "colorMode": "critical",
            "op": "lt"
          }
        ]
      },
      {
        "id": 7,
        "title": "üö® GA Core Gate Status Details",
        "type": "table",
        "targets": [
          {
            "expr": "ga_core_gate_status",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background",
              "filterable": true
            },
            "mappings": [
              {
                "options": {
                  "PASS": {
                    "color": "green"
                  },
                  "FAIL": {
                    "color": "red"
                  },
                  "WARNING": {
                    "color": "yellow"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 8,
        "title": "ü§ñ Copilot NL‚ÜíCypher Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "copilot_nl_success_rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0.7,
            "max": 1.0,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0.7
                },
                {
                  "color": "yellow",
                  "value": 0.8
                },
                {
                  "color": "green", 
                  "value": 0.9
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 9,
        "title": "‚ö° System Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(query_duration_seconds{quantile=\"0.95\"})",
            "refId": "A",
            "legendFormat": "Query Latency p95"
          },
          {
            "expr": "rate(http_requests_total[5m])",
            "refId": "B",
            "legendFormat": "Request Rate"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 20
        }
      },
      {
        "id": 10,
        "title": "üîç Hypothesis Rigor Score",
        "type": "stat",
        "targets": [
          {
            "expr": "hypothesis_rigor_score_avg",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 10,
            "unit": "short",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 6
                },
                {
                  "color": "green",
                  "value": 8
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 20
        },
        "description": "Average hypothesis rigor score based on competing hypotheses, weights, and residual unknowns"
      },
      {
        "id": 11,
        "title": "üíæ Data Quality Score",
        "type": "stat", 
        "targets": [
          {
            "expr": "data_quality_score_overall",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0.8,
            "max": 1.0,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0.8
                },
                {
                  "color": "yellow",
                  "value": 0.9
                },
                {
                  "color": "green",
                  "value": 0.95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 20
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(ga_core_release_decision[1d]) > 0",
          "iconColor": "red",
          "name": "GA Release Decisions",
          "titleFormat": "GA Core Decision",
          "textFormat": "Release status changed"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "7d",
            "value": "7d"
          },
          "hide": 0,
          "includeAll": false,
          "label": "Time Range",
          "multi": false,
          "name": "timerange",
          "options": [
            {
              "selected": false,
              "text": "1d",
              "value": "1d"
            },
            {
              "selected": true,
              "text": "7d", 
              "value": "7d"
            },
            {
              "selected": false,
              "text": "30d",
              "value": "30d"
            }
          ],
          "query": "1d,7d,30d",
          "queryValue": "",
          "type": "custom"
        }
      ]
    }
  }
}