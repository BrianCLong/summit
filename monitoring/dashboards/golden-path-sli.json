        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "id": 5,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "(\n  1 - (\n    sum(rate(graphql_requests_total{status=\"success\"}[1h])) /\n    sum(rate(graphql_requests_total[1h]))\n  )\n) / 0.01",
          "interval": "",
          "legendFormat": "GraphQL Error Budget Burn (1h)",
          "refId": "A"
        },
        {
          "expr": "(\n  histogram_quantile(0.95, rate(graphql_request_duration_seconds_bucket[1h])) - 2\n) / 8",
          "interval": "",
