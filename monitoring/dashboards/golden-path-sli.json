        {
          "expr": "(\n  1 - (\n    sum(rate(http_requests_total{route=\"/monitoring/health\", status_code=\"200\"}[1h])) /\n    sum(rate(http_requests_total{route=\"/monitoring/health\"}[1h]))\n  )\n) / 0.001",
          "interval": "",
          "legendFormat": "Health Check Error Budget Burn (1h)",
          "refId": "C"
        }
      ],
      "title": "Error Budget Burn Rate (Golden Path SLOs)",
      "type": "timeseries"
    }
