    },
    {
      "datasource": "Prometheus",
      "description": "Error budget burn rate for Golden Path SLIs",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "",
