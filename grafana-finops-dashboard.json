
{
  "__inputs": [
    {
      "name": "DS_BUILD_LAKE",
      "label": "Build Lake Datasource",
      "description": "",
      "type": "datasource",
      "pluginId": "clickhouse",
      "pluginName": "ClickHouse"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.2.0"
    },
    {
      "type": "datasource",
      "id": "clickhouse",
      "name": "ClickHouse",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Total Spend vs Forecast",
      "type": "timeseries",
      "datasource": "${DS_BUILD_LAKE}",
      "gridPos": { "h": 8, "w": 18, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 2,
      "title": "Spend by Tenant (Last 30d)",
      "type": "piechart",
      "datasource": "${DS_BUILD_LAKE}",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "options": { "displayLabels": ["name", "percent"], "legend": { "displayMode": "list", "placement": "right"} }
    },
    {
      "id": 3,
      "title": "Spend Variance %",
      "type": "stat",
      "datasource": "${DS_BUILD_LAKE}",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 8 },
      "options": { "colorMode": "background", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "orange", "value": 0 }, { "color": "red", "value": 10 }] }
        }
      }
    },
    {
      "id": 4,
      "title": "Budget Overrun Predictions (Next 7d)",
      "type": "table",
      "datasource": "${DS_BUILD_LAKE}",
      "gridPos": { "h": 12, "w": 16, "x": 8, "y": 8 }
    },
    {
      "id": 5,
      "title": "Cost Breakdown (Compute/Storage/Egress)",
      "type": "timeseries",
      "datasource": "${DS_BUILD_LAKE}",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "stacking": { "mode": "normal" } }
    },
    {
      "id": 6,
      "title": "Rightsizing Recommendations",
      "type": "table",
      "datasource": "${DS_BUILD_LAKE}",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 12 }
    }
  ],
  "schemaVersion": 37,
  "style": "dark",
  "tags": ["maestro", "finops"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "All", "value": "$__all" },
        "datasource": "${DS_BUILD_LAKE}",
        "definition": "SELECT DISTINCT tenant_name FROM cost_by_day",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "tenant",
        "options": [],
        "query": "SELECT DISTINCT tenant_name FROM cost_by_day",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 0,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-30d", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Maestro FinOps Dashboard",
  "uid": "maestro-finops-v1",
  "version": 1,
  "weekStart": ""
}
