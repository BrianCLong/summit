{
  "problemMatcher": [
    {
      "owner": "test-guards",
      "pattern": [
        {
          "regexp": ".*Focused test detected: \\.only is forbidden.*",
          "severity": "error",
          "message": 1
        }
      ]
    },
    {
      "owner": "console-error-guard",
      "pattern": [
        {
          "regexp": ".*console\\.error called in tests is forbidden.*",
          "severity": "error",
          "message": 1
        }
      ]
    },
    {
      "owner": "jest-open-handles",
      "pattern": [
        {
          "regexp": ".*Jest did not exit one second after the test run has completed.*",
          "severity": "warning",
          "message": 1
        }
      ]
    }
  ]
}
