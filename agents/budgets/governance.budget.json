{
  "$schema": "https://summit.dev/schemas/agent-budget-manifest.schema.json",
  "schemaVersion": "2025-02-15",
  "agentId": "governance",
  "owner": "Agent Control Systems",
  "description": "Governance and policy agent enforcing compliance baselines with conservative budgets.",
  "deterministicLimits": {
    "maxTokens": 30000,
    "maxCallsPerRun": 20,
    "maxWallClockMs": 120000,
    "maxConcurrentRuns": 1,
    "maxDailyRuns": 120,
    "maxNetworkKB": 1024
  },
  "risk": {
    "inherent": 4,
    "controlBaseline": 3,
    "residual": 1,
    "riskTier": "low",
    "riskCeiling": 4,
    "gates": ["policy-check", "budget-validation"]
  },
  "telemetry": {
    "auditLogPath": "agents/audit/logs/agent-audit.log",
    "emitBudgetEvents": true,
    "sampleRate": 1
  }
}
