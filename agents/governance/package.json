{
  "name": "@summit/agent-governance",
  "version": "1.0.0",
  "description": "Misuse-aware orchestration framework for AI agent fleets with IC FY28 compliance",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./policy-engine": {
      "import": "./dist/policy-engine/AgentPolicyEngine.js",
      "types": "./dist/policy-engine/AgentPolicyEngine.d.ts"
    },
    "./orchestration": {
      "import": "./dist/orchestration/PromptChainOrchestrator.js",
      "types": "./dist/orchestration/PromptChainOrchestrator.d.ts"
    },
    "./incident-response": {
      "import": "./dist/incident-response/IncidentResponseManager.js",
      "types": "./dist/incident-response/IncidentResponseManager.d.ts"
    },
    "./hallucination-audit": {
      "import": "./dist/hallucination-audit/HallucinationAuditor.js",
      "types": "./dist/hallucination-audit/HallucinationAuditor.d.ts"
    },
    "./rollback": {
      "import": "./dist/rollback/RollbackManager.js",
      "types": "./dist/rollback/RollbackManager.d.ts"
    },
    "./provenance": {
      "import": "./dist/provenance/AIProvenanceManager.js",
      "types": "./dist/provenance/AIProvenanceManager.d.ts"
    },
    "./compliance": {
      "import": "./dist/compliance/ICFY28ComplianceValidator.js",
      "types": "./dist/compliance/ICFY28ComplianceValidator.d.ts"
    },
    "./dashboard": {
      "import": "./dist/dashboard/GovernanceDashboardService.js",
      "types": "./dist/dashboard/GovernanceDashboardService.d.ts"
    }
  },
  "files": [
    "dist",
    "policies"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ai",
    "agent",
    "governance",
    "opa",
    "policy",
    "slsa",
    "provenance",
    "compliance",
    "ic-fy28",
    "hallucination",
    "incident-response",
    "rollback",
    "summit",
    "intelgraph"
  ],
  "author": "IntelGraph Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrianCLong/summit.git",
    "directory": "agents/governance"
  }
}
