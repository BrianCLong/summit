{
  "name": "@intelgraph/plan-eval-platform",
  "version": "0.1.0",
  "description": "Evaluation-first platform for Summit/IntelGraph with cost-aware routing and telemetry",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./eval": "./src/eval/index.ts",
    "./runtime": "./src/runtime/index.ts",
    "./routing": "./src/routing/index.ts",
    "./safety": "./src/safety/index.ts"
  },
  "types": "./src/index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "eval:run": "tsx src/eval/cli.ts",
    "benchmark:report": "tsx benchmark/generate-report.ts"
  },
  "dependencies": {
    "@ga-graphai/common-types": "file:../../ga-graphai/packages/common-types",
    "@ga-graphai/cost-guard": "file:../../ga-graphai/packages/cost-guard",
    "@ga-graphai/maestro-conductor": "file:../../ga-graphai/packages/maestro-conductor",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "eval",
    "benchmark",
    "routing",
    "telemetry",
    "llm",
    "tool-use",
    "cost-optimization"
  ],
  "license": "MIT"
}
