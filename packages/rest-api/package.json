{
  "name": "@intelgraph/rest-api",
  "version": "1.0.0",
  "description": "Unified REST API framework with OpenAPI 3.0 specification",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rest-api",
    "openapi",
    "swagger",
    "api-framework"
  ],
  "dependencies": {
    "express": "^5.1.0",
    "openapi3-ts": "^4.1.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "morgan": "^1.10.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^10.0.0",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "jest": "^30.2.0",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./router": {
      "types": "./dist/router.d.ts",
      "import": "./dist/router.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js"
    },
    "./openapi": {
      "types": "./dist/openapi/index.d.ts",
      "import": "./dist/openapi/index.js"
    }
  }
}
