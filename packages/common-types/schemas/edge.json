{
  "$ref": "#/definitions/edge",
  "definitions": {
    "edge": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "relatesTo",
            "locatedAt",
            "participatesIn",
            "derivedFrom",
            "mentions",
            "contradicts",
            "supports",
            "enrichedFrom"
          ]
        },
        "sourceId": {
          "type": "string"
        },
        "targetId": {
          "type": "string"
        },
        "observedAt": {
          "type": "string",
          "format": "date-time"
        },
        "tenantId": {
          "type": "string"
        },
        "policyLabels": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        },
        "provenance": {
          "type": "object",
          "properties": {
            "chain": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": ["chain"],
          "additionalProperties": false,
          "default": {
            "chain": []
          }
        }
      },
      "required": [
        "id",
        "type",
        "sourceId",
        "targetId",
        "observedAt",
        "tenantId"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}
