{
  "$ref": "#/definitions/run",
  "definitions": {
    "run": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "connectorId": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": ["PENDING", "RUNNING", "SUCCEEDED", "FAILED", "DLQ"]
        },
        "attempt": {
          "type": "integer",
          "minimum": 0
        },
        "scheduleId": {
          "type": "string"
        },
        "startedAt": {
          "type": "string",
          "format": "date-time"
        },
        "finishedAt": {
          "type": "string",
          "format": "date-time"
        },
        "error": {
          "type": "string"
        },
        "metrics": {
          "type": "object",
          "properties": {
            "items": {
              "type": "integer",
              "minimum": 0
            },
            "bytes": {
              "type": "integer",
              "minimum": 0
            },
            "durationMs": {
              "type": "integer",
              "minimum": 0
            }
          },
          "required": ["items", "bytes", "durationMs"],
          "additionalProperties": false
        },
        "provenanceRef": {
          "type": "string"
        },
        "dedupeKey": {
          "type": "string"
        },
        "tenantId": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "connectorId",
        "status",
        "attempt",
        "startedAt",
        "metrics",
        "provenanceRef",
        "tenantId"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}
