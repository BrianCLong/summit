{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Message",
  "type": "object",
  "properties": {
    "message_id": { "type": "string" },
    "team_id": { "type": "string" },
    "from_agent_id": { "type": "string" },
    "to": { "type": "string" },
    "type": { "type": "string" },
    "payload": { "type": "object" },
    "created_at": { "type": "string", "format": "date-time" },
    "signature": { "type": "string" },
    "nonce": { "type": "string" },
    "causal_parent": { "type": "string" },
    "redaction_map": { "type": "object" }
  },
  "required": ["message_id", "team_id", "from_agent_id", "type", "payload", "created_at"]
}
