{
  "name": "@intelgraph/knowledge-graph-search",
  "version": "1.0.0",
  "description": "Knowledge graph search with graph traversal, pattern matching, and SPARQL",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "neo4j-driver": "^6.0.1",
    "cypher-query-builder": "^6.1.0",
    "sparqljs": "^3.7.3",
    "graphology": "^0.25.4",
    "graphology-shortest-path": "^2.0.2",
    "graphology-traversal": "^0.3.1",
    "lodash": "^4.17.21",
    "lru-cache": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/lodash": "^4.14.202",
    "@types/sparqljs": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "knowledge-graph",
    "graph-search",
    "neo4j",
    "cypher",
    "sparql",
    "graph-traversal",
    "pattern-matching"
  ],
  "author": "IntelGraph Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
