{
  "name": "@summit/cli",
  "version": "4.2.3",
  "description": "Command-line interface for the Summit platform",
  "keywords": [
    "summit",
    "cli",
    "governance",
    "compliance",
    "policy"
  ],
  "author": "Summit Platform Team",
  "license": "MIT",
  "type": "module",
  "bin": {
    "summit": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@summit/foundry": "workspace:^",
    "@summit/orchestrator": "workspace:^",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "@summit/agent-registry": "workspace:*",
    "@intelgraph/golden-path": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^9.39.2",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/summit-platform/summit.git",
    "directory": "packages/cli"
  }
}
