{
  "name": "@intelgraph/feature-flags",
  "version": "2.0.0",
  "description": "Comprehensive feature flag system with provider abstraction, caching, and analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./providers/launchdarkly": {
      "types": "./dist/providers/launchdarkly.d.ts",
      "import": "./dist/providers/launchdarkly.js"
    },
    "./providers/unleash": {
      "types": "./dist/providers/unleash.d.ts",
      "import": "./dist/providers/unleash.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    }
  },
  "dependencies": {
    "launchdarkly-node-server-sdk": "^7.0.4",
    "unleash-client": "^5.0.0",
    "ioredis": "^5.4.1",
    "winston": "^3.11.0",
    "prom-client": "^15.1.3",
    "eventemitter3": "^5.0.1",
    "murmurhash": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/murmurhash": "^2.0.0",
    "typescript": "^5.3.3",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ts-jest": "^29.0.0",
    "@types/react": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
