{
  "name": "@summit/platform-data",
  "version": "1.0.0",
  "description": "Data lineage, mock data factory, and privacy utilities (P36-40)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lineage": {
      "import": "./dist/lineage/index.js",
      "types": "./dist/lineage/index.d.ts"
    },
    "./mock": {
      "import": "./dist/mock/index.js",
      "types": "./dist/mock/index.d.ts"
    },
    "./privacy": {
      "import": "./dist/privacy/index.js",
