{
  "name": "@summit/sdk",
  "version": "4.2.3",
  "description": "Official TypeScript SDK for the Summit platform",
  "keywords": [
    "summit",
    "governance",
    "compliance",
    "sdk",
    "api-client"
  ],
  "author": "Summit Platform Team",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./governance": {
      "types": "./dist/governance.d.ts",
      "import": "./dist/governance.js"
    },
    "./compliance": {
      "types": "./dist/compliance.d.ts",
      "import": "./dist/compliance.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/summit-platform/summit.git",
    "directory": "packages/sdk/typescript"
  },
  "bugs": {
    "url": "https://github.com/summit-platform/summit/issues"
  },
  "homepage": "https://github.com/summit-platform/summit#readme"
}
