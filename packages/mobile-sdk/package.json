{
  "name": "@intelgraph/mobile-sdk",
  "version": "4.2.3",
  "description": "Shared mobile SDK for IntelGraph cross-platform applications with offline-first architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./offline": {
      "types": "./dist/offline/index.d.ts",
      "import": "./dist/offline/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "idb": "^8.0.1",
    "zod": "^4.1.12",
    "@nozbe/watermelondb": "^0.27.1",
    "react-native-keychain": "^8.1.1",
    "react-native-biometrics": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsup": "^8.3.7",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3",
    "@babel/plugin-proposal-decorators": "^7.23.6"
  },
  "keywords": [
    "mobile",
    "sdk",
    "offline",
    "sync",
    "cross-platform",
    "watermelondb"
  ],
  "author": "IntelGraph Team",
  "license": "MIT"
}
