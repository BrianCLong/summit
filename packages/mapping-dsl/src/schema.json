{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "version": { "type": "string" },
    "entities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": { "type": "string" },
          "fields": { "type": "object" }
        },
        "required": ["type", "fields"]
      }
    },
    "relationships": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": { "type": "string" },
          "from": { "type": "string" },
          "to": { "type": "string" }
        },
        "required": ["type", "from", "to"]
      }
    }
  },
  "required": ["version", "entities", "relationships"]
}
