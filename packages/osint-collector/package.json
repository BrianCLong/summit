{
  "name": "@intelgraph/osint-collector",
  "version": "1.0.0",
  "description": "OSINT Collector for IntelGraph Platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "osint",
    "intelligence",
    "collection",
    "intelgraph",
    "data-pipeline"
  ],
  "author": "IntelGraph Team",
  "license": "BUSL-1.1",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^22.0.0",
    "zod": "^3.22.4",
    "@intelgraph/types": "workspace:*",
    "@intelgraph/config": "workspace:*",
    "@intelgraph/observability": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "vitest": "^1.3.0",
    "tsup": "^8.0.0",
    "@intelgraph/tsconfig": "workspace:*"
  },
  "publishConfig": {
    "access": "restricted"
  }
}