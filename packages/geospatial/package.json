{
  "name": "@intelgraph/geospatial",
  "version": "1.0.0-rc.1",
  "description": "Geospatial intelligence with satellite imagery processing, Neo4j graph storage, and change detection for IntelGraph GEOINT operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "benchmark": "jest --testPathPattern=benchmark"
  },
  "keywords": [
    "geospatial",
    "GEOINT",
    "GIS",
    "intelligence",
    "satellite-imagery",
    "change-detection",
    "neo4j",
    "raster-processing",
    "gdal",
    "remote-sensing"
  ],
  "author": "IntelGraph Team",
  "license": "MIT",
  "dependencies": {
    "@turf/turf": "^7.1.0",
    "@types/geojson": "^7946.0.14",
    "geojson": "^0.5.0",
    "shpjs": "^6.0.0",
    "xml2js": "^0.6.2",
    "proj4": "^2.12.1",
    "fast-xml-parser": "^5.3.4",
    "neo4j-driver": "^5.27.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.27",
    "jest": "^30.2.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "neo4j-driver": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "neo4j-driver": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
