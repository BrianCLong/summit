{
  "name": "@intelgraph/summit-cli",
  "version": "0.1.0",
  "description": "Unified CLI for IntelGraph Summit platform - canonical entry point for humans and AI agents",
  "type": "module",
  "bin": {
    "summit": "./bin/summit.js"
  },
  "exports": {
    ".": "./src/index.js",
    "./lib/*": "./src/lib/*",
    "./commands/*": "./src/commands/*"
  },
  "scripts": {
    "dev": "node --watch bin/summit.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "intelgraph",
    "summit",
    "orchestration",
    "devops"
  ],
  "author": "IntelGraph Team",
  "license": "UNLICENSED",
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^10.2.2",
    "execa": "^9.4.0",
    "listr2": "^8.2.5",
    "yaml": "^2.6.0",
    "zod": "^3.24.1",
    "cosmiconfig": "^9.0.0",
    "table": "^6.8.2",
    "strip-ansi": "^7.1.0",
    "log-symbols": "^7.0.0",
    "figures": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
