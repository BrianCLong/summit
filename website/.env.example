# Environment variables for Topicality Website
# Copy this to .env.local for local development

# Analytics (optional, disabled if empty)
NEXT_PUBLIC_ANALYTICS_ENABLED=

# API Endpoints (if needed)
NEXT_PUBLIC_API_URL=

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://topicality.co
NEXT_PUBLIC_SITE_NAME=Topicality

# Feature Flags
NEXT_PUBLIC_FEATURE_SUMMIT=true
NEXT_PUBLIC_FEATURE_LABS=true
