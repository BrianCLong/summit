{
  "name": "@intelgraph/er-service",
  "version": "1.0.0",
  "description": "IntelGraph Entity Resolution & Identity Service",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*' --exec 'npx tsx' src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:benchmarks": "jest --testMatch='**/benchmarks/**/*.test.ts'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "compression": "1.8.1",
    "cors": "2.8.5",
    "express": "^5.1.0",
    "geolib": "3.3.4",
    "helmet": "8.1.0",
    "luxon": "^3.7.2",
    "natural": "8.1.0",
    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "uuid": "13.0.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "24.10.1",
    "@types/supertest": "6.0.3",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "eslint": "9.39.1",
    "jest": "30.2.0",
    "jest-extended": "7.0.0",
    "nodemon": "3.1.11",
    "supertest": "7.1.4",
    "ts-jest": "29.4.5",
    "tsx": "latest",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
