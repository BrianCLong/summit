{
  "name": "@intelgraph/registry-tools",
  "version": "1.0.0",
  "description": "Air-gapped registry infrastructure tools for IntelGraph platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'cosign/**/*.ts' 'slsa/**/*.ts' 'sync/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "cosign:verify": "ts-node cosign/cosign-verifier.ts verify",
    "slsa:verify": "ts-node slsa/slsa3-verifier.ts verify",
    "sync:export": "ts-node sync/offline-sync.ts export",
    "sync:import": "ts-node sync/offline-sync.ts import"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "cosign",
    "slsa",
    "sync",
    "policies"
  ],
  "keywords": [
    "container",
    "registry",
    "harbor",
    "cosign",
    "slsa",
    "security",
    "air-gapped"
  ],
  "author": "IntelGraph Platform Team",
  "license": "UNLICENSED",
  "private": true
}
