{
  "title": "Retractions SLA",
  "panels": [
    {
      "type": "timeseries",
      "title": "Processed",
      "targets": [{ "expr": "sum by (status) (rate(retractions_processed_total[5m]))" }]
    },
    {
      "type": "timeseries",
      "title": "Processing Time (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(retractions_processing_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "SLA Breaches",
      "targets": [{ "expr": "increase(retractions_sla_breach_total[24h])" }]
    }
  ]
}
