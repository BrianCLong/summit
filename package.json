{
  "name": "intelgraph-platform",
  "version": "1.0.0",
  "description": "Next-generation intelligence analysis platform with AI-augmented graph analytics",
  "main": "server/server.js",
  "scripts": {
    "setup": "chmod +x scripts/setup.sh && scripts/setup.sh",
    "cleanup": "chmod +x scripts/cleanup-repository.sh && scripts/cleanup-repository.sh",
    "dev": "pnpm --parallel --filter \"./server\" --filter \"./client\" dev",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up",
    "test": "jest",
    "test:connector-contracts": "CONNECTOR_CONTRACTS=1 jest --config tests/connector-contracts/jest.config.cjs",
    "test:server": "pnpm --filter intelgraph-server test",
    "test:client": "pnpm --filter @intelgraph/web test",
    "test:e2e": "npx playwright test",
    "test:smoke": "npm run test:smoke:backend && npm run test:smoke:frontend",
    "test:smoke:backend": "cd server && npm run test:smoke",
    "test:smoke:frontend": "cd client && npm run test:smoke",
    "test:quick": "turbo run test --filter='!*integration*' --filter='!*e2e*' --cache-dir=.turbo -- --testPathPattern='(sample|mathUtils|stringUtils)\\.test' --maxWorkers=2 --bail || echo 'Quick tests passed or jest not configured'",
    "policy:test": "bash ./scripts/policy-test.sh",
    "test:unit": "npm run test",
    "test:integration": "cd server && npm run test:integration",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "deploy:dev": "scripts/deploy.sh dev",
    "deploy:staging": "scripts/deploy.sh staging",
    "deploy:prod": "scripts/deploy.sh prod",
    "backup": "scripts/backup.sh",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:server": "cd server && npm run lint",
    "lint:client": "cd client && npm run lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:server": "cd server && tsc --noEmit",
    "typecheck:client": "cd client && tsc --noEmit",
    "format": "prettier -w . && ruff format",
    "pr:risk": "node scripts/pr-risk-score.js",
    "db:migrate": "cd server && npm run db:migrate",
    "db:seed": "cd server && npm run db:seed",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "db:migrate:neo4j": "npx ts-node --esm scripts/migrate.ts",
    "health": "chmod +x scripts/health-check.sh && scripts/health-check.sh",
    "install:pm": "node scripts/select-pm.js",
    "summitctl": "cd tools/summitctl && npm run start --",
    "e2e": "playwright test",
    "quickstart": "bash scripts/quickstart.sh",
    "graphql:schema:check": "graphql-inspector diff packages/graphql/schema.graphql origin/main:packages/graphql/schema.graphql --fail-on-breaking",
    "graphql:codegen": "graphql-codegen",
    "db:pg:generate": "prisma generate",
    "db:pg:migrate": "prisma migrate deploy",
    "db:pg:status": "prisma migrate status",
    "db:knex:migrate": "knex migrate:latest --knexfile packages/db/knex/knexfile.cjs",
    "db:knex:rollback": "knex migrate:rollback --knexfile packages/db/knex/knexfile.cjs",
    "db:neo4j:migrate": "node scripts/neo4j-migrate.js",
    "fuzz": "ts-node tooling/fuzz/cli.ts",
    "fuzz:ci": "FUZZ_MODE=ci ts-node tooling/fuzz/cli.ts",
    "adapters:pack": "node --experimental-strip-types adapters/reference/packaging.ts",
    "changeset": "changeset",
    "release": "changeset version && changeset publish",
    "check:golden": "npx tsx scripts/check-golden-paths.ts",
    "scaffold": "npx tsx scripts/scaffold/index.ts",
    "sdk:generate": "pnpm --filter @intelgraph/sdk generate",
    "precommit": "lint-staged",
    "db:prisma:diff": "prisma migrate diff --from-url $DATABASE_URL --to-schema-datamodel prisma/schema.prisma",
    "db:knex:smoke": "knex migrate:down && knex migrate:up",
    "k6:policy": "k6 run k6/policy-hot-path.js"
  },
  "keywords": [
    "intelligence-analysis",
    "graph-analytics",
    "neo4j",
    "react",
    "graphql",
    "ai-ml",
    "collaboration",
    "investigation"
  ],
  "author": "IntelGraph Team",
  "license": "MIT",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20.19.0 <21",
    "npm": ">=8.0.0",
    "pnpm": "^9.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/compat": "2.0.0",
    "@eslint/js": "9.39.1",
    "@graphql-codegen/cli": "6.1.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@graphql-codegen/typescript-react-apollo": "4.3.4",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.67.3",
    "@opentelemetry/exporter-prometheus": "0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.208.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-metrics": "2.2.0",
    "@opentelemetry/sdk-node": "0.208.0",
    "@opentelemetry/semantic-conventions": "1.38.0",
    "@playwright/test": "1.57.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "concurrently": "9.2.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "license-checker": "25.0.1",
    "lint-staged": "16.2.7",
    "markdownlint-cli": "0.45.0",
    "prettier": "3.7.4",
    "prom-client": "15.1.3",
    "semantic-release": "24.2.9",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "*.py": [
      "ruff check --fix",
      "ruff format"
    ],
    "*.graphql": [
      "graphql-schema-linter"
    ],
    "*.{md,css,scss,json,yml,yaml}": [
      "prettier -w"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "axios": "1.13.2",
    "cross-env": "10.1.0",
    "docx": "9.5.1",
    "dotenv": "17.2.3",
    "jest-extended": "4.0.2",
    "neo4j-driver": "5.28.2",
    "pg": "8.16.3",
    "redis": "5.10.0",
    "yaml": "2.6.1",
    "ws": "8.18.3"
  },
  "private": true
}
