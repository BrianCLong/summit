{
  "name": "summit-monorepo",
  "version": "0.0.1",
  "description": "Summit Monorepo",
  "scripts": {
    "dev": "turbo run dev --cache-dir=.turbo --continue",
    "dev:server": "cd server && pnpm run dev",
    "dev:client": "cd client && pnpm run dev",
    "start": "turbo run start --cache-dir=.turbo --continue",
    "test": "turbo run test --cache-dir=.turbo --continue",
    "test:server": "cd server && pnpm run test",
    "test:client": "cd client && pnpm run test",
    "test:e2e": "playwright test",
    "test:ci": "pnpm run lint && pnpm run typecheck && pnpm run test",
    "test:smoke": "node scripts/smoke-test.js",
    "build": "turbo run build --cache-dir=.turbo --continue",
    "build:server": "cd server && pnpm run build",
    "build:client": "cd client && pnpm run build",
    "lint": "turbo run lint --cache-dir=.turbo --continue",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "typecheck": "turbo run typecheck --cache-dir=.turbo --continue",
    "typecheck:server": "cd server && tsc --noEmit",
    "typecheck:client": "cd client && tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "turbo run clean --cache-dir=.turbo --continue",
    "clean:all": "rimraf node_modules **/node_modules",
    "install:ci": "pnpm install --frozen-lockfile",
    "db:migrate": "cd server && pnpm run db:migrate",
    "db:seed": "cd server && pnpm run db:seed",
    "db:reset": "pnpm run db:migrate && pnpm run db:seed",
    "changeset": "changeset",
    "release": "changeset version && changeset publish",
    "precommit": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "bootstrap": "make bootstrap",
    "metrics:all": "pnpm run metrics:report && pnpm run metrics:complexity && pnpm run metrics:loc",
    "metrics:report": "node scripts/maintainability-report.js",
    "metrics:complexity": "eslint --config .eslintrc.complexity.cjs 'server/src/**/*.{js,ts}' 'client/src/**/*.{js,jsx,ts,tsx}'",
    "metrics:loc": "cloc server/src client/src apps packages services --exclude-dir=node_modules,dist,build,coverage,.turbo,archive",
    "security:scan": "pnpm run security:audit && pnpm run security:lint",
    "security:audit": "pnpm audit --audit-level=moderate",
    "security:lint": "eslint --config .eslintrc.security.cjs 'server/src/**/*.{js,ts}' --max-warnings 0"
  },
  "keywords": [
    "intelligence-analysis",
    "graph-analytics",
    "neo4j",
    "react",
    "graphql",
    "ai-ml",
    "collaboration",
    "investigation"
  ],
  "author": "IntelGraph Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.18",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.0",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@apollo/client": "^4.0.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@opentelemetry/api": "^1.9.0",
    "@playwright/test": "^1.56.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "concurrently": "^9.2.0",
    "eslint": "9.39.1",
    "eslint-plugin-jest": "^29.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-watch-typeahead": "^3.0.1",
    "lint-staged": "^16.1.5",
    "markdownlint-cli": "^0.46.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.11.0",
    "cross-env": "^10.1.0",
    "docx": "^9.5.1",
    "dotenv": "^17.2.3",
    "graphql-depth-limit": "^1.1.0",
    "jest-extended": "^7.0.0",
    "js-yaml": "^4.1.1",
    "ioredis": "^5.4.1",
    "neo4j-driver": "6.0.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.2",
    "redis": "^5.8.1",
    "ws": "^8.18.3"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.3.3",
      "@opentelemetry/api": "1.7.0",
      "@opentelemetry/sdk-node": "0.48.0",
      "@opentelemetry/sdk-trace-base": "1.19.0",
      "@opentelemetry/sdk-metrics": "1.19.0",
      "@opentelemetry/resources": "1.19.0",
      "@opentelemetry/propagator-b3": "1.19.0",
      "@opentelemetry/propagator-jaeger": "1.19.0",
      "@opentelemetry/exporter-trace-otlp-http": "0.48.0",
      "@opentelemetry/exporter-metrics-otlp-http": "0.48.0",
      "@opentelemetry/auto-instrumentations-node": "0.48.0",
      "graphql-redis-subscriptions": "^3.6.0",
      "express-rate-limit": "8.1.0",
      "neo4j-driver": "6.0.1",
      "eslint": "9.39.1",
      "parse-url": "^8.1.0",
      "parse-path": "^5.0.0",
      "parse-duration": ">=2.1.3",
      "electron": ">=35.7.5",
      "nanoid": ">=5.0.9",
      "body-parser": ">=2.2.1",
      "protobufjs": ">=7.2.5",
      "next": ">=16.0.7",
      "esbuild": "^0.25.0",
      "glob": "^11.1.0",
      "moment": "^2.29.4",
      "isolated-vm": "^5.0.0"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx gitleaks protect --staged --verbose && pnpm run lint && pnpm run typecheck && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.py": [
      "ruff check --fix",
      "ruff format"
    ],
    "*.graphql": [
      "prettier --write"
    ],
    "*.{md,css,scss,json,yml,yaml}": [
      "prettier --write"
    ],
    "*.{html,xml}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  }
}
