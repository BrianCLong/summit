{
  "name": "summit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "server:dev": "nodemon --watch server --ext ts,json --exec node --loader ts-node/esm server/index.ts",
    "server:start": "node dist/server/index.js"
  },
  "dependencies": {
    "@neo4j/graphql": "^5.6.1",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "graphql": "^16.8.1",
    "lucide-react": "^0.453.0",
    "neo4j-driver": "^5.25.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "xxhash-wasm": "^1.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "pnpm": {
    "overrides": {
      "xxhash": "npm:xxhash-wasm@^1.0.2"
    }
  }
}
