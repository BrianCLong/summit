{
  "name": "summit",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "prettier --write .",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "ga:verify": "node scripts/ga/ga-verify-runner.mjs",
    "test:integration": "pnpm -r --filter=!./packages/* test:integration",
    "test:e2e": "playwright test",
    "regulatory:build": "node scripts/regulatory/build.mjs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*",
    "agents/*",
    "cli",
    "server"
  ]
}
