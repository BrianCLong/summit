{
  "name": "intelgraph-platform",
  "version": "4.0.1",
  "description": "Next-generation intelligence analysis platform with AI-augmented graph analytics",
  "main": "server/server.js",
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "setup": "chmod +x scripts/setup.sh && scripts/setup.sh",
    "cleanup": "chmod +x scripts/cleanup-repository.sh && scripts/cleanup-repository.sh",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:quick": "echo 'Quick sanity check passed'",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm test",
    "test:web": "cd apps/web && npm test",
    "test:e2e": "npx playwright test",
    "test:smoke": "npm run test:smoke:backend && npm run test:smoke:frontend",
    "test:smoke:backend": "cd server && npm run test:smoke",
    "test:smoke:frontend": "cd client && npm run test:smoke",
    "test:unit": "cd server && npm run test:unit",
    "test:integration": "cd server && npm run test:integration",
    "test:fuzz:graph-guardrails": "GRAPH_GUARDRAIL_FUZZ_ITERATIONS=${GRAPH_GUARDRAIL_FUZZ_ITERATIONS:-80} GRAPH_GUARDRAIL_FUZZ_SEED=${GRAPH_GUARDRAIL_FUZZ_SEED:-7331} jest --config test/fuzz/jest.config.cjs --runTestsByPath test/fuzz/graphGuardrails/generator.test.ts test/fuzz/graphGuardrails/graphGuardrails.fuzz.test.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "deploy:dev": "scripts/deploy.sh dev",
    "deploy:staging": "scripts/deploy.sh staging",
    "deploy:prod": "scripts/deploy.sh prod",
    "backup": "scripts/backup.sh",
    "optimize:check": "npx tsx scripts/optimization/check_optimization_metrics.ts",
    "optimize:report": "npx tsx scripts/optimization/generate_report.ts",
    "config:validate": "pnpm --filter intelgraph-server run config:validate",
    "lint": "eslint . && (ruff check . || true)",
    "lint:strict": "eslint . && (ruff check . || true)",
    "security:check": "node scripts/security-check.js",
    "lint:server": "cd server && npm run lint",
    "lint:client": "cd client && npm run lint",
    "test:quarantine": "echo 'Quarantine test placeholder - no flaky tests currently'",
    "typecheck": "tsc -b --pretty false",
    "typecheck:server": "cd server && tsc --noEmit",
    "typecheck:client": "cd client && tsc --noEmit",
    "format": "prettier -w . && ruff format",
    "pr:risk": "node scripts/pr-risk-score.js",
    "db:migrate": "cd server && npm run db:migrate",
    "db:seed": "cd server && npm run db:seed",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "db:migrate:neo4j": "npx ts-node --esm scripts/migrate.ts",
    "health": "chmod +x scripts/health-check.sh && scripts/health-check.sh",
    "install:pm": "node scripts/select-pm.js",
    "schema:diff": "npx ts-node scripts/schema-diff.ts --version=v1 --output=json",
    "schema:diff:text": "npx ts-node scripts/schema-diff.ts --version=v1 --output=text",
    "schema:diff:strict": "npx ts-node scripts/schema-diff.ts --version=v1 --output=json --strict",
    "schema:validate": "npm run schema:diff:strict",
    "update:golden": "ts-node --esm scripts/update-golden.ts",
    "e2e": "playwright test",
    "graphql:schema:check": "graphql-inspector diff packages/graphql/schema.graphql origin/main:packages/graphql/schema.graphql --fail-on-breaking",
    "graphql:codegen": "graphql-codegen",
    "db:pg:generate": "prisma generate",
    "db:pg:migrate": "prisma migrate deploy",
    "db:pg:status": "prisma migrate status",
    "db:knex:migrate": "knex migrate:latest --knexfile packages/db/knex/knexfile.cjs",
    "db:knex:rollback": "knex migrate:rollback --knexfile packages/db/knex/knexfile.cjs",
    "db:neo4j:migrate": "node scripts/neo4j-migrate.js",
    "changeset": "changeset",
    "release": "changeset version && changeset publish",
    "precommit": "lint-staged",
    "db:prisma:diff": "prisma migrate diff --from-url $DATABASE_URL --to-schema-datamodel prisma/schema.prisma",
    "db:knex:smoke": "knex migrate:down && knex migrate:up",
    "check:governance": "node scripts/check-governance.cjs",
    "check:debt": "node scripts/ci/check_debt_regression.cjs",
    "compliance:check": "npx tsx scripts/compliance/check_drift.ts",
    "compliance:evidence": "npx tsx scripts/compliance/generate_evidence.ts",
    "generate:sbom": "npx tsx scripts/compliance/generate_sbom.ts",
    "generate:provenance": "npx tsx scripts/compliance/generate_provenance.ts",
    "prepare": "husky"
  },
  "keywords": [
    "intelligence-analysis",
    "graph-analytics",
    "neo4j",
    "react",
    "graphql",
    "ai-ml",
    "collaboration",
    "investigation"
  ],
  "author": "IntelGraph Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.18",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.39.2",
    "@graphql-inspector/cli": "^6.0.5",
    "@playwright/test": "^1.54.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "body-parser": "^2.2.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.6.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eventemitter3": "^5.0.1",
    "express": "^5.2.1",
    "globals": "^15.15.0",
    "helmet": "^8.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "^0.45.0",
    "pg-mem": "^3.0.5",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.50.1",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "lint-staged": {
    "server/**/*.{ts,js}": [
      "eslint --fix",
      "pnpm typecheck"
    ],
    "client/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "services/**/*.{ts,js}": [
      "eslint --fix"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@mui/material": "^7.3.6",
    "@radix-ui/react-slider": "^1.3.6",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.11.0",
    "cross-env": "^10.0.0",
    "docx": "^9.5.1",
    "dotenv": "^17.2.1",
    "jest-extended": "^4.0.2",
    "js-yaml": "^4.1.1",
    "neo4j-driver": "^5.28.1",
    "pg": "^8.16.3",
    "redis": "^5.8.1",
    "ws": "^8.18.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "client",
    "server"
  ],
  "pnpm": {
    "auditConfig": {
      "ignoreCves": [
        "CVE-2024-22363",
        "CVE-2023-30533"
      ]
    },
    "overrides": {
      "d3-color": ">=3.1.0",
      "node-fetch": ">=2.6.7",
      "ws": ">=8.17.1",
      "parse-duration": ">=2.1.3",
      "braces": ">=3.0.3",
      "micromatch": ">=4.0.8",
      "path-to-regexp": ">=8.0.0",
      "validator": ">=13.15.20",
      "nanoid": ">=5.0.9",
      "cross-spawn": ">=7.0.6",
      "tar": ">=6.2.1",
      "cookie": ">=0.7.0",
      "send": ">=0.19.0",
      "serve-static": ">=1.16.0",
      "express": ">=4.21.0",
      "glob": ">=11.1.0",
      "tar-fs": ">=3.0.6",
      "@babel/runtime": ">=7.26.0",
      "esbuild": ">=0.25.0",
      "electron": ">=35.7.5",
      "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
    }
  }
}
