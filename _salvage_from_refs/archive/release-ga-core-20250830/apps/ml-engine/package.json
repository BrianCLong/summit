{
  "name": "@intelgraph/ml-engine",
  "version": "1.0.0",
  "description": "Machine Learning Engine for IntelGraph Entity Resolution",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "setup-python": "cd src/python && pip3 install -r requirements.txt",
    "train": "node dist/cli.js train",
    "evaluate": "node dist/cli.js evaluate"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "http-proxy-middleware": "^2.0.6",
    "pg": "^8.11.3",
    "neo4j-driver": "^5.15.0",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "bull": "^4.12.2",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.7",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "machine-learning",
    "entity-resolution",
    "nlp",
    "graph-analytics",
    "intelligence-analysis"
  ],
  "author": "IntelGraph Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}