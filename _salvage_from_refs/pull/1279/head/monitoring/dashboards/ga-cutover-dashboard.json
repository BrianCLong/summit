{
  "dashboard": {
    "id": null,
    "title": "IntelGraph GA Cutover - Mission Control",
    "tags": ["intelgraph", "ga-cutover", "slo", "sprint-26"],
    "style": "dark",
    "timezone": "UTC",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "üöÄ GA CUTOVER STATUS",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"intelgraph-gateway\"} * 100",
            "legendFormat": "API Availability %",
            "refId": "A"
          },
          {
            "expr": "ga_cutover_phase",
            "legendFormat": "Cutover Phase",
            "refId": "B"
          },
          {
            "expr": "ga_go_nogo_status * 100",
            "legendFormat": "Go/No-Go Score %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "lcd",
              "orientation": "horizontal"
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "Pre-Cutover"},
                  "1": {"text": "Canary 1%"},
                  "2": {"text": "Canary 5%"},
                  "3": {"text": "Canary 25%"},
                  "4": {"text": "Security Enable"},
                  "5": {"text": "Full Deploy"},
                  "6": {"text": "Validation"},
                  "7": {"text": "Complete"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Cutover Phase"},
              "properties": [
                {"id": "unit", "value": "none"},
                {"id": "mappings", "value": [
                  {
                    "options": {
                      "0": {"text": "Pre-Cutover", "color": "blue"},
                      "1": {"text": "Canary 1%", "color": "yellow"},
                      "2": {"text": "Canary 5%", "color": "yellow"},
                      "3": {"text": "Canary 25%", "color": "orange"},
                      "4": {"text": "Security Enable", "color": "purple"},
                      "5": {"text": "Full Deploy", "color": "orange"},
                      "6": {"text": "Validation", "color": "green"},
                      "7": {"text": "Complete", "color": "green"}
                    },
                    "type": "value"
                  }
                ]}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "üéØ SLO Compliance (Real-time)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(graphql_request_duration_seconds_bucket{operation_type=\"query\"}[5m])) * 1000",
            "legendFormat": "GraphQL Read p95 (ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(graphql_request_duration_seconds_bucket{operation_type=\"mutation\"}[5m])) * 1000",
            "legendFormat": "GraphQL Write p95 (ms)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(neo4j_query_duration_seconds_bucket{hops=\"1\"}[5m])) * 1000",
            "legendFormat": "Neo4j 1-hop p95 (ms)",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, rate(neo4j_query_duration_seconds_bucket{hops=~\"2|3\"}[5m])) * 1000",
            "legendFormat": "Neo4j Multi-hop p95 (ms)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 300},
                {"color": "red", "value": 400}
              ]
            },
            "unit": "ms"
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "GraphQL Read.*"},
              "properties": [
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": 0},
                    {"color": "yellow", "value": 350},
                    {"color": "red", "value": 450}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "GraphQL Write.*"},
              "properties": [
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": 0},
                    {"color": "yellow", "value": 700},
                    {"color": "red", "value": 850}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "Neo4j Multi-hop.*"},
              "properties": [
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": 0},
                    {"color": "yellow", "value": 1200},
                    {"color": "red", "value": 1500}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 3,
        "title": "‚ö†Ô∏è ROLLBACK TRIGGERS",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "ALERTS{alertname=~\"GA.*\",alertstate=\"firing\"}",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Time"
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": false,
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "alertname": "Alert",
                "severity": "Severity",
                "summary": "Description"
              }
            }
          }
        ]
      },
      {
        "id": 4,
