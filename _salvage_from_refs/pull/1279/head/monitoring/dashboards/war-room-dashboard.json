{
  "dashboard": {
    "id": null,
    "title": "GA Cutover War Room - Mission Control",
    "tags": ["ga-cutover", "war-room", "mission-control", "sprint-26"],
    "style": "dark",
    "timezone": "UTC",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 37,
    "version": 1,
    "links": [
      {
        "type": "dashboards",
        "title": "Related Dashboards",
        "includeVars": true,
        "keepTime": true,
        "tags": ["ga-cutover"]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Cutover Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(cutover_stage_transitions_total[1m]) > 0",
          "titleFormat": "Cutover Stage: {{stage}}%",
          "textFormat": "{{operator}} initiated {{action}}",
          "iconColor": "green"
        },
        {
          "name": "Rollback Triggers",
          "datasource": "prometheus",
          "enable": true,
          "expr": "increase(rollback_triggers_total[1m]) > 0",
          "titleFormat": "üö® ROLLBACK TRIGGER",
          "textFormat": "{{trigger_type}}: {{reason}}",
          "iconColor": "red"
        }
      ]
    },
    "panels": [
      {
        "title": "üöÄ GA CUTOVER STATUS",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "cutover_active",
            "legendFormat": "Cutover Active",
            "refId": "A"
          },
          {
            "expr": "cutover_current_stage",
            "legendFormat": "Current Stage %",
            "refId": "B"
          },
          {
            "expr": "cutover_rollback_armed",
            "legendFormat": "Auto-Rollback Armed",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "mappings": [
              {"value": 0, "text": "INACTIVE", "color": "gray"},
              {"value": 1, "text": "ACTIVE", "color": "green"}
            ],
            "thresholds": {
              "steps": [
                {"color": "gray", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "title": "üéØ SLO COMPLIANCE - REAL TIME",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 0},
        "targets": [
          {
            "expr": "min(slo_compliance_p0) * 100",
            "legendFormat": "P0 SLOs",
            "refId": "A"
          },
          {
            "expr": "min(slo_compliance_p1) * 100",
            "legendFormat": "P1 SLOs",
            "refId": "B"
          },
          {
            "expr": "min(error_budget_remaining_days)",
            "legendFormat": "Error Budget (days)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 99},
                {"color": "green", "value": 99.9}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Error Budget (days)"},
              "properties": [
                {"id": "unit", "value": "d"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 7},
                    {"color": "green", "value": 14}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "title": "‚ö†Ô∏è ROLLBACK TRIGGERS",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 0},
        "targets": [
          {
            "expr": "rollback_trigger_error_budget",
            "legendFormat": "Error Budget Burn",
            "refId": "A"
          },
          {
            "expr": "rollback_trigger_latency_degradation",
            "legendFormat": "Latency Degradation",
            "refId": "B"
          },
          {
            "expr": "rollback_trigger_security_failure",
            "legendFormat": "Security Failure",
            "refId": "C"
          },
          {
            "expr": "rollback_trigger_cost_spike",
            "legendFormat": "Cost Spike",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "mappings": [
              {"value": 0, "text": "OK", "color": "green"},
              {"value": 1, "text": "TRIGGERED", "color": "red"}
            ],
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "options": {
          "orientation": "vertical",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "title": "üìä PERFORMANCE METRICS - P95 LATENCIES",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(graphql_request_duration_seconds_bucket{operation_type=\"query\"}[1m]))",
            "legendFormat": "GraphQL Read p95 (‚â§350ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(graphql_request_duration_seconds_bucket{operation_type=\"mutation\"}[1m]))",
            "legendFormat": "GraphQL Write p95 (‚â§700ms)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(neo4j_query_duration_seconds_bucket{type=\"1hop\"}[1m]))",
            "legendFormat": "Neo4j 1-hop p95 (‚â§300ms)",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, rate(opa_decision_duration_seconds_bucket[1m]))",
            "legendFormat": "OPA Decision p95 (‚â§25ms)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.3},
                {"color": "red", "value": 0.7}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["lastNotNull"]}
        }
      },
      {
        "title": "üî• ERROR RATES & AVAILABILITY",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "rate(graphql_request_total{status=~\"5..\"}[1m]) * 100",
            "legendFormat": "GraphQL 5xx Error Rate %",
            "refId": "A"
          },
          {
            "expr": "rate(ingest_errors_total[1m]) * 100",
            "legendFormat": "Ingest Error Rate %",
            "refId": "B"
          },
          {
            "expr": "100 - (rate(graphql_request_total{status=~\"5..\"}[1m]) / rate(graphql_request_total[1m]) * 100)",
            "legendFormat": "API Availability %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 3,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 99.5},
                {"color": "green", "value": 99.9}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*Error Rate.*"},
              "properties": [
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 0.1},
                    {"color": "red", "value": 0.5}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["lastNotNull"]}
        }
      },
      {
        "title": "üí∞ COST GUARDRAILS STATUS",
        "type": "bargauge",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 14},
