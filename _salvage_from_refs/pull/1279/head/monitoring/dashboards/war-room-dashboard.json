      },
      {
        "title": "üìà ADAPTIVE SAMPLING & COST REDUCTION",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 14},
        "targets": [
          {
            "expr": "intelgraph:sampling_rate_current * 100",
