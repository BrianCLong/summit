{
  "name": "@summit/admin-cli",
  "version": "1.0.0",
  "description": "Unified Admin & SRE CLI for IntelGraph platform operations",
  "type": "module",
  "bin": {
    "summit-admin": "./dist/cli.js",
    "ig-admin": "./dist/cli.js"
  },
  "scripts": {
