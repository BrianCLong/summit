{
  "id": "valid-minimal",
  "version": 1,
  "assumptions": {
    "traffic_pattern": "constant"
  },
  "components": [
    {
      "name": "api-gateway",
      "type": "api",
      "replicas": 3
    },
    {
      "name": "main-db",
      "type": "db",
      "replicas": 2
    }
  ],
  "connections": [
    {
      "from": "api-gateway",
      "to": "main-db",
      "protocol": "http"
    }
  ]
}
