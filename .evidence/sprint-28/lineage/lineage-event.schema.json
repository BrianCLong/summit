{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Lineage Event",
  "type": "object",
  "required": [
    "event_type",
    "output_dataset",
    "input_datasets",
    "job_id",
    "code_version",
    "policy_version",
    "actor",
    "trace_id",
    "timestamp"
  ],
  "additionalProperties": false,
  "properties": {
    "event_type": {
      "type": "string",
      "const": "lineage_event"
    },
    "output_dataset": {
      "type": "string"
    },
    "input_datasets": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "job_id": {
      "type": "string"
    },
    "code_version": {
      "type": "string"
    },
    "policy_version": {
      "type": "string"
    },
    "actor": {
      "type": "string"
    },
    "trace_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    }
  }
}
