{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "compound.stamp.schema.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "created_at": { "type": "string", "format": "date-time" },
    "git": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "sha": { "type": "string" },
        "ref": { "type": "string" }
      },
      "required": ["sha", "ref"]
    }
  },
  "required": ["created_at", "git"]
}
