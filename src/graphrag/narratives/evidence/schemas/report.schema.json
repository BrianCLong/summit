  "$id": "https://summit.local/schemas/narratives/report.schema.json",
  "type": "object",
  "minProperties": 1,
  "required": ["summary"],
  "properties": {
    "evidence_id": { "type": "string" },
    "summary": { "type": "string" },
    "status": { "type": "string" },
