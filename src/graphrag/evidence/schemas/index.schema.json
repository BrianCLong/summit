{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://summit.ai/schemas/evidence/index.schema.json",
  "title": "Competitor Decode Evidence Index",
  "type": "object",
  "required": ["entries"],
  "properties": {
    "entries": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["evidenceId", "files"],
        "properties": {
          "evidenceId": { "type": "string" },
          "files": {
            "type": "array",
            "items": { "type": "string" },
            "minItems": 1
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
