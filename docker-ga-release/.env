# Maestro Conductor Docker GA Release - Environment Variables

# PostgreSQL
POSTGRES_DB=maestro_dev
POSTGRES_USER=maestro
POSTGRES_PASSWORD=dev_password
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Neo4j
NEO4J_URI=bolt://neo4j:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=dev_password

# OPA
OPA_URL=http://opa:8181

# Kafka/Redpanda
KAFKA_BOOTSTRAP_SERVERS=ingest:9092

# OTEL
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4317
OTEL_SERVICE_NAME=maestro-conductor

# API
API_PORT=4000
API_HOST=0.0.0.0

# Frontend
FRONTEND_PORT=3000

# Prometheus
PROMETHEUS_PORT=9090

# Grafana
GRAFANA_PORT=3001
GF_SECURITY_ADMIN_PASSWORD=dev_password

# Jaeger
JAEGER_PORT=16686

# Build Environment
NODE_ENV=development
BUILD_ENV=summit-prod