{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "useUnknownInCatchVariables": false,
    "noImplicitThis": false,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "allowJs": false
  }
}
