{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "useUnknownInCatchVariables": false,
    "noImplicitThis": false,
    "skipLibCheck": true,
    "noEmitOnError": false
  },
  "exclude": [
    "node_modules",
    "../node_modules",
    "dist",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "tests/**",
    "../tests/**",
    "src/tests/**",
    "src/routes/geoint.js"
  ]
}
