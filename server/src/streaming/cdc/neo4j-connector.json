{
  "name": "neo4j-connector",
  "config": {
    "connector.class": "org.neo4j.connectors.kafka.source.Neo4jSourceConnector",
    "kafka.bootstrap.servers": "kafka:9092",
    "neo4j.server.uri": "bolt://neo4j:7687",
    "neo4j.authentication.basic.username": "neo4j",
    "neo4j.authentication.basic.password": "password",
    "neo4j.source.query": "MATCH (n) RETURN n",
    "neo4j.topic": "neo4j-updates",
    "neo4j.streaming.poll.interval.msecs": 5000,
    "neo4j.streaming.from": "NOW"
  }
}
