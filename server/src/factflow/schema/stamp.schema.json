{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FactFlow Stamp",
  "type": "object",
  "properties": {
    "job_id": {
      "type": "string"
    },
    "hash": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "generated_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["job_id", "hash", "version", "generated_at"]
}
