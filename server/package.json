{
  "name": "intelgraph-server",
  "version": "4.2.3",
  "private": true,
  "description": "IntelGraph Server Application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node build.mjs",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:ci": "cross-env NODE_OPTIONS='--experimental-vm-modules' pnpm exec jest --config jest.config.ts --passWithNoTests --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "db:seed": "tsx scripts/seed-db.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.504.0",
    "@aws-sdk/s3-request-presigner": "^3.504.0",
    "@intelgraph/events": "workspace:*",
    "@neo4j/graphql": "^5.2.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "neo4j-driver": "^5.17.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "sanitize-filename": "^1.6.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
