{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "lib": ["ES2022"],
    "types": [],
    "typeRoots": ["./types"]
  },
  "ts-node": {
    "esm": true
  },
  "files": [
    "types/shims.d.ts",
    "src/config/logger.ts",
    "src/conductor/api/conductor-routes.ts",
    "src/conductor/web-orchestration/orchestration-service.ts",
    "src/conductor/premium-routing/premium-model-router.ts",
    "src/conductor/premium-routing/__tests__/premium-model-router.test.ts"
  ]
}
