{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./src",
    "allowJs": true,
    "noEmit": false,
    "lib": [
      "ES2022"
    ],
    "types": [
      "node"
    ],
    "strict": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "ts-node": {
    "esm": true
  }
}
