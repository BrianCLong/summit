{
  "dashboard": {
    "id": null,
    "title": "Maestro Conductor v24.4.0 - Release Overview",
    "description": "Comprehensive dashboard for Sprint +3 features: Provenance, Abuse Detection, Cost Management, RTBF, and Tenant SLOs",
    "tags": ["v24.4.0", "release", "overview", "E18", "E19", "E20", "E21", "E22"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "üè∑Ô∏è Release Status",
        "type": "stat",
        "targets": [
          {
            "expr": "maestro_version_info",
            "format": "table",
            "legendFormat": "v{{ $labels.version }}"
          }
        ],
        "gridPos": {"h": 3, "w": 6, "x": 0, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [
              {"options": {"v24.4.0": {"color": "green", "index": 0, "text": "v24.4.0 ACTIVE"}}}
            ]
          }
        }
      },
      {
        "id": 2, 
        "title": "üéØ Epic Completion Status",
        "type": "bargauge",
        "targets": [
          {
            "expr": "vector(100)",
            "legendFormat": "E18 - Provenance"
          },
          {
            "expr": "vector(100)", 
            "legendFormat": "E19 - Abuse Detection"
          },
          {
            "expr": "vector(100)",
            "legendFormat": "E20 - Cost Management"
          },
          {
            "expr": "vector(100)",
            "legendFormat": "E21 - RTBF"
          },
          {
            "expr": "vector(100)",
            "legendFormat": "E22 - Tenant SLOs"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "showUnfilled": false
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "continuous-GrYlRd"},
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "title": "‚ö†Ô∏è Active Alerts by Epic", 
        "type": "stat",
        "targets": [
          {
            "expr": "count by (epic) (ALERTS{alertstate=\"firing\", epic=~\"E1[8-9]|E2[0-2]\"})",
            "legendFormat": "{{ $labels.epic }}"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "üìä Overall System Health",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(up{job=~\"provenance|abuse-guard|cost-service|rtbf|slo-service\"})",
            "legendFormat": "Healthy Services"
          },
          {
            "expr": "sum((up{job=~\"provenance|abuse-guard|cost-service|rtbf|slo-service\"} == 0))",
            "legendFormat": "Unhealthy Services"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "pieType": "pie",
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },

      {
        "id": 10,
        "title": "üîê E18 - Provenance & Trust",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 7},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Hash Chain Integrity",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(provenance_ledger_verify_success_total[5m]) / rate(provenance_ledger_verify_total[5m]) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99.9}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 12,
        "title": "Daily Merkle Root Signing",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(provenance_merkle_root_signed_total[1d])",
            "legendFormat": "Roots Signed"
          },
          {
            "expr": "increase(provenance_merkle_root_sign_failures_total[1d])",
            "legendFormat": "Sign Failures"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 4, "y": 8},
        "yAxes": [
          {"min": 0, "unit": "short"},
          {"show": false}
        ]
      },
      {
        "id": 13,
        "title": "Ledger Append Latency (P95)",
        "type": "graph", 
        "targets": [
          {
            "expr": "histogram_quantile(0.95, provenance_ledger_append_duration_seconds_bucket)",
            "legendFormat": "P95 Latency"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "yAxes": [
          {"min": 0, "unit": "s"},
          {"show": false}
        ]
      },
      {
        "id": 14,
        "title": "SLSA Attestations",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(slsa_attestations_verified_total[1h])",
            "legendFormat": "Verified"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },

      {
        "id": 20,
        "title": "üõ°Ô∏è E19 - Abuse Detection & Mitigation",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 13},
        "collapsed": false
      },
      {
        "id": 21,
        "title": "Rate Anomaly Detection",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(abuseguard_throttles_total[5m])",
            "legendFormat": "Throttles/sec - {{ $labels.tenant_id }}"
          },
          {
            "expr": "rate(abuseguard_false_positive_total[5m])", 
            "legendFormat": "False Positives/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 14},
        "yAxes": [
          {"min": 0, "unit": "ops"},
          {"show": false}
        ]
      },
      {
        "id": 22,
        "title": "Graph Complexity Blocks",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(graph_guard_block_total[5m])",
            "legendFormat": "Blocked/sec - {{ $labels.tenant_id }}"
          },
          {
            "expr": "histogram_quantile(0.95, graph_query_complexity_score_bucket)",
            "legendFormat": "P95 Complexity Score"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 14},
        "yAxes": [
          {"min": 0, "unit": "short"},
          {"show": false}
        ]
      },
      {
        "id": 23,
        "title": "Malformed Ingest Rejection",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ingest_malformed_total[5m]) / rate(ingest_requests_total[5m]) * 100",
            "legendFormat": "Rejection Rate"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 14},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent"
          }
        }
      },

      {
        "id": 30,
        "title": "üí∞ E20 - Cost Management & Partitioning",
        "type": "row", 
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 19},
        "collapsed": false
      },
      {
        "id": 31,
        "title": "Per-Tenant Cost Tracking",
        "type": "graph",
        "targets": [
          {
            "expr": "tenant_cost_total",
            "legendFormat": "Cost - {{ $labels.tenant_id }}"
          }
        ],
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 20},
        "yAxes": [
          {"min": 0, "unit": "currencyUSD"},
          {"show": false}
        ]
      },
      {
        "id": 32,
        "title": "Cost Forecast Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(tenant_cost_forecast_accuracy_percentage)",
            "legendFormat": "Average Accuracy"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 85},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 33,
        "title": "Active Partition Migrations", 
        "type": "stat",
        "targets": [
          {
            "expr": "sum(tenant_partition_migrations_active)",
            "legendFormat": "Active Migrations"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },

      {
        "id": 40,
        "title": "üîê E21 - RTBF at Scale",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25},
        "collapsed": false
      },
      {
        "id": 41,
        "title": "RTBF Job Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rtbf_job_duration_seconds_bucket)",
            "legendFormat": "P95 Duration"
          },
          {
            "expr": "7200",
            "legendFormat": "SLA (2h)"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 26},
        "yAxes": [
          {"min": 0, "unit": "s"},
          {"show": false}
        ]
      },
      {
        "id": 42,
        "title": "Records Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rtbf_items_processed_total[5m])",
            "legendFormat": "Records/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 26},
        "yAxes": [
          {"min": 0, "unit": "ops"},
          {"show": false}
        ]
      },
      {
        "id": 43,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "rtbf_workers_active",
            "legendFormat": "Workers"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },

      {
        "id": 50,
        "title": "üìä E22 - Tenant SLO Observability", 
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 31},
        "collapsed": false
      },
      {
        "id": 51,
        "title": "Error Budget Burn Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "tenant_error_budget_burn_rate_5m",
            "legendFormat": "5m Burn - {{ $labels.tenant_id }}"
          },
          {
            "expr": "tenant_error_budget_burn_rate_1h", 
            "legendFormat": "1h Burn - {{ $labels.tenant_id }}"
          }
        ],
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 32},
        "yAxes": [
          {"min": 0, "unit": "short"},
          {"show": false}
        ]
      },
      {
        "id": 52,
        "title": "SLO Compliance by Tenant",
        "type": "heatmap",
        "targets": [
          {
            "expr": "tenant_slo_compliance",
            "legendFormat": "{{ $labels.tenant_id }} - {{ $labels.slo_type }}"
          }
        ],
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 32},
        "heatmap": {
          "hideZeroBuckets": true,
          "highlightCards": true,
          "cardColor": "#b4ff00",
          "cardPadding": null,
          "cardRound": null,
          "color": {"mode": "spectrum"},
          "colorScale": "sqrt",
          "colorScheme": "interpolateSpectral",
          "exponent": 0.5,
          "gridBucketSize": null
        }
      },
      {
        "id": 53,
        "title": "Synthetic Test Results",
        "type": "graph",
        "targets": [
          {
            "expr": "synthetic_test_slo_compliance",
            "legendFormat": "Compliance % - {{ $labels.environment }}"
          },
          {
            "expr": "synthetic_test_tenants_with_violations",
            "legendFormat": "Violations - {{ $labels.environment }}"
          }
        ],
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 36},
        "yAxes": [
          {"min": 0, "unit": "short"},
          {"show": false}
        ]
      },

      {
        "id": 60,
        "title": "üîç Cross-Epic Correlations",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 41},
        "collapsed": false
      },
      {
        "id": 61,
        "title": "Abuse Events vs Cost Spikes",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(abuseguard_throttles_total[5m])",
            "legendFormat": "Throttles/sec",
            "yAxis": 1
          },
          {
            "expr": "rate(tenant_cost_spike_total[5m])",
            "legendFormat": "Cost Spikes/sec",
            "yAxis": 2  
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 42},
        "yAxes": [
          {"min": 0, "unit": "ops", "label": "Abuse Events"},
          {"min": 0, "unit": "ops", "label": "Cost Events"}
        ]
      },
      {
        "id": 62,
        "title": "SLO Violations vs RTBF Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(tenant_slo_violations_total[5m])",
            "legendFormat": "SLO Violations"
          },
          {
            "expr": "rtbf_jobs_active",
            "legendFormat": "Active RTBF Jobs"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 42},
        "yAxes": [
          {"min": 0, "unit": "short"},
          {"show": false}
        ]
      },
      {
        "id": 63,
        "title": "Provenance vs System Load",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(provenance_ledger_append_total[5m])",
            "legendFormat": "Ledger Appends/sec"
          },
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "HTTP Requests/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 42},
        "yAxes": [
          {"min": 0, "unit": "ops"},
          {"show": false}
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "query": "label_values(up, environment)",
          "current": {"value": "production", "text": "production"},
          "options": [],
          "includeAll": false,
          "multi": false
        },
        {
          "name": "tenant",
          "type": "query", 
          "query": "label_values(tenant_slo_availability, tenant_id)",
          "current": {"value": "All", "text": "All"},
          "options": [],
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(maestro_version_info[5m])",
          "iconColor": "rgba(0, 211, 255, 1)",
          "titleFormat": "Deployment",
          "textFormat": "Version: {{ $labels.version }}"
        },
        {
          "name": "Critical Alerts",
          "datasource": "Prometheus", 
          "expr": "ALERTS{severity=\"critical\", epic=~\"E1[8-9]|E2[0-2]\"}",
          "iconColor": "rgba(255, 96, 96, 1)",
          "titleFormat": "{{ $labels.alertname }}",
          "textFormat": "{{ $labels.summary }}"
        }
      ]
    }
  }
}