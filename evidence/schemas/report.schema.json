{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "run_id": { "type": "string" },
    "item_slug": { "type": "string" },
    "evd_ids": { "type": "array", "items": { "type": "string" } },
    "summary": {
      "type": "object",
      "required": ["what_ran", "result"],
      "properties": {
        "what_ran": { "type": "string" },
        "result": { "type": "string" }
      }
    },
    "artifacts": {
      "type": "object",
      "properties": {
        "metrics": { "type": "string" },
        "stamp": { "type": "string" }
      }
    }
  }
}
