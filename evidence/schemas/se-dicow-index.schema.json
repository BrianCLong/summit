{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["evidence"],
  "properties": {
    "evidence": {
      "type": "object",
      "patternProperties": {
        "^EVD-SE-DICOW-[A-Z0-9-]+$": {
          "type": "object",
          "required": ["report", "metrics", "stamp"],
          "properties": {
            "report": { "type": "string" },
            "metrics": { "type": "string" },
            "stamp": { "type": "string" }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": true
}
