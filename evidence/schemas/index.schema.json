{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["version", "items"],
  "properties": {
    "version": { "type": "integer", "const": 1 },
    "items": {
      "type": "object",
      "patternProperties": {
        "^EVD-[A-Z0-9-]+$": { "type": "string" }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
