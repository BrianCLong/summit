{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "swarm metrics",
  "type": "object",
  "required": ["run_id", "metrics"],
  "properties": {
    "run_id": {"type": "string"},
    "metrics": {
      "type": "object",
      "required": ["agentsSpawned", "stepsExecuted", "toolCalls", "wallMs"],
      "properties": {
        "agentsSpawned": {"type": "integer"},
        "stepsExecuted": {"type": "integer"},
        "toolCalls": {"type": "integer"},
        "wallMs": {"type": "integer"}
      }
    }
  },
  "additionalProperties": false
}
