{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["evidence_id", "generated_at"],
  "properties": {
    "evidence_id": {
      "type": "string",
      "pattern": "^EVD-SE-DICOW-[A-Z0-9-]+$"
    },
    "generated_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": true
}
