{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "items": {
      "type": "object",
      "patternProperties": {
        "^EVD-[A-Z0-9-]+$": {
          "type": "object",
          "properties": {
            "files": {
              "type": "array",
              "items": { "type": "string" }
            }
          },
          "required": ["files"]
        }
      }
    }
  },
  "required": ["items"]
}
