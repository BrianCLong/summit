{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Summit RAG Evidence Report",
  "type": "object",
  "required": ["run_id", "evd_ids", "artifacts"],
  "properties": {
    "run_id": {"type": "string"},
    "evd_ids": {
      "type": "array",
      "items": {"type": "string"}
    },
    "artifacts": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["path", "sha256"],
        "properties": {
          "path": {"type": "string"},
          "sha256": {"type": "string"}
        }
      }
    }
  },
  "additionalProperties": false
}
