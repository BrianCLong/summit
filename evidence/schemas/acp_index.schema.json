{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["version", "items"],
  "properties": {
    "version": { "type": "integer" },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "files"],
        "properties": {
          "id": { "type": "string" },
          "files": { "type": "array", "items": { "type": "string" } }
        }
      }
    }
  }
}
