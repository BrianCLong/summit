{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": ["item", "run", "results"],
  "properties": {
    "item": {"type": "string"},
    "run": {
      "type": "object",
      "additionalProperties": false,
      "required": ["mode", "git"],
      "properties": {
        "mode": {"type": "string"},
        "git": {
          "type": "object",
          "additionalProperties": false,
          "required": ["sha"],
          "properties": {
            "sha": {"type": "string"}
          }
        }
      }
    },
    "results": {
      "type": "array",
      "items": {"type": "object"}
    }
  }
}
