{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Intel PR Stack",
  "type": "object",
  "required": ["pr_stack"],
  "properties": {
    "pr_stack": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "required": ["title", "description", "gated"],
        "properties": {
          "title": { "type": "string" },
          "description": { "type": "string" },
          "gated": { "type": "boolean" }
        }
      }
    }
  }
}
