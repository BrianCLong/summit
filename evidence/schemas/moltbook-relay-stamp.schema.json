{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Moltbook Relay Evidence Stamp",
  "type": "object",
  "required": ["evidence_id", "generated_at_utc"],
  "properties": {
    "evidence_id": {
      "type": "string",
      "pattern": "^EVD-moltbook-relay-[A-Z]+-\\d{3}$"
    },
    "generated_at_utc": {
      "type": "string",
      "format": "date-time"
    },
    "node_version": { "type": "string" }
  },
  "additionalProperties": true
}
