{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "item_slug": {
      "type": "string",
      "const": "cti-briefing-cloud-supplychain"
    },
    "claims": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "pattern": "^ITEM:CLAIM-[0-9]{2}$|^Summit original$"
          },
          "source": {
            "type": "string"
          },
          "snippet": {
            "type": "string"
          },
          "url": {
            "type": "string",
            "format": "uri"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "mitre_attack_id": {
            "type": "string"
          }
        },
        "required": ["id", "source", "snippet", "tags"]
      }
    }
  },
  "required": ["item_slug", "claims"],
  "additionalProperties": false
}
