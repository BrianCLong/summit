"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
const costEstimator_1 = require("../nl2cypher/costEstimator");
describe('cost estimator', () => {
    it('assigns higher cost for filtered queries', () => {
        expect((0, costEstimator_1.estimateCost)({ type: 'find', label: 'Person', filter: null })).toBe(1);
        expect((0, costEstimator_1.estimateCost)({
            type: 'find',
            label: 'Person',
            filter: { property: 'name', value: 'Alice' },
        })).toBe(2);
        expect((0, costEstimator_1.estimateCost)({ type: 'count', label: 'Person', filter: null })).toBe(1);
        expect((0, costEstimator_1.estimateCost)({
            type: 'count',
            label: 'Person',
            filter: { property: 'name', value: 'Alice' },
        })).toBe(2);
    });
});
