{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://summit.intelgraph/feed-processor/neo4j-entity.schema.json",
  "title": "FeedProcessorNeo4jEntity",
  "type": "object",
  "required": ["id"],
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "entity_type": {
      "type": "string"
    },
    "extracted_at": {
      "type": "string",
      "format": "date-time"
    },
    "properties": {
      "type": "object",
      "additionalProperties": true
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": true
}
