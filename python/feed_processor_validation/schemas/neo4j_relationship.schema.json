{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://summit.intelgraph/feed-processor/neo4j-relationship.schema.json",
  "title": "FeedProcessorNeo4jRelationship",
  "type": "object",
  "required": ["source", "target", "type"],
  "properties": {
    "source": {
      "type": "string",
      "minLength": 1
    },
    "target": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "type": "string",
      "pattern": "^[A-Z0-9_]+$"
    },
    "properties": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
