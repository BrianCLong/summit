{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://summit.intelgraph/feed-processor/postgres-record.schema.json",
  "title": "FeedProcessorPostgresRecord",
  "type": "object",
  "required": ["job_id", "record_id", "source_type", "payload"],
  "properties": {
    "job_id": {
      "type": "string",
      "minLength": 1
    },
    "record_id": {
      "type": "string",
      "minLength": 1
    },
    "source_type": {
      "type": "string",
      "enum": ["csv", "json", "elasticsearch", "esri", "api"]
    },
    "target_graph": {
      "type": "string",
      "minLength": 1
    },
    "authority_id": {
      "type": "string",
      "minLength": 1
    },
    "data_source_id": {
      "type": "string",
      "minLength": 1
    },
    "payload": {
      "type": "object"
    },
    "ingested_at": {
      "type": "string",
      "format": "date-time"
    },
    "metadata": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
