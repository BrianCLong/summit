{
  "title": "Postgres Slow Queries",
  "panels": [
    {
      "type": "table",
      "title": "Top slow queries",
      "targets": [
        {
          "expr": "topk(10, rate(db_query_latency_seconds_sum{store='postgres'}[5m]) / rate(db_query_latency_seconds_count{store='postgres'}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Pool wait",
      "targets": [{ "expr": "db_pool_wait_seconds" }]
    }
  ]
}
