{
  "title": "Performance Overview",
  "timezone": "utc",
  "panels": [
    {
      "type": "timeseries",
      "title": "RPS by journey",
      "targets": [
        {
          "expr": "sum by (journey) (rate(http_requests_total{run!=""}[1m]))",
          "legendFormat": "{{journey}}"
        }
      ],
      "id": 1
    },
    {
      "type": "timeseries",
      "title": "Latency p95/p99",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{run!=""}[5m])) by (le, journey))",
          "legendFormat": "p95 {{journey}}"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{run!=""}[5m])) by (le, journey))",
          "legendFormat": "p99 {{journey}}"
        }
      ],
      "id": 2
    },
    {
      "type": "stat",
      "title": "Headroom",
      "targets": [
        {
          "expr": "avg(perf_headroom_ratio)",
          "legendFormat": "headroom"
        }
      ],
      "id": 3
    },
    {
      "type": "stat",
      "title": "RPS at p95 budget",
      "targets": [
        {
          "expr": "avg(perf_rps_at_p95_budget)",
          "legendFormat": "rps@budget"
        }
      ],
      "id": 4
    },
    {
      "type": "timeseries",
      "title": "Error rate",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\", run!=""}[5m])) / sum(rate(http_requests_total{run!=""}[5m]))",
          "legendFormat": "error rate"
        }
      ],
      "id": 5
    }
  ],
  "schemaVersion": 37,
  "version": 1
}
