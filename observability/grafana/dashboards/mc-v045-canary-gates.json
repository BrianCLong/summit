{
  "dashboard": {
    "id": null,
    "title": "MC Platform v0.4.5 - Canary Gates & Adaptive Excellence",
    "tags": ["mc-platform", "v0.4.5", "canary-gates", "adaptive", "quantum"],
    "style": "dark",
    "timezone": "UTC",
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(mc_qam_quantum_minutes_used, tenant)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "current": {
            "value": "TENANT_001",
            "text": "TENANT_001"
          }
        },
        {
          "name": "environment",
          "type": "custom",
          "options": [
            {"text": "Staging", "value": "staging"},
            {"text": "Production", "value": "production"}
          ],
          "current": {
            "value": "staging",
            "text": "Staging"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "🚦 Canary Gate Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "mc:opt_delta_score_24h > 0",
            "legendFormat": "Delta Gate",
            "refId": "A"
          },
          {
            "expr": "mc:correctness_floor_ok",
            "legendFormat": "Correctness Floor",
            "refId": "B"
          },
          {
            "expr": "mc:autotune_weight_valid",
            "legendFormat": "Weight Sanity",
            "refId": "C"
          },
          {
            "expr": "mc:qam_budget_utilization_percent{tenant=~\"$tenant\"} < 100",
            "legendFormat": "Budget Gate",
            "refId": "D"
          },
          {
            "expr": "mc:qam_p95_regression_percent < 5",
            "legendFormat": "Performance Gate",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "FAIL"},
              {"type": "value", "value": "1", "text": "PASS"}
            ],
            "unit": "none"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "📈 Delta Score Trend (24h)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "mc:opt_delta_score_24h",
            "legendFormat": "Delta Score 24h",
            "refId": "A"
          },
          {
            "expr": "0",
            "legendFormat": "Threshold (0)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": -0.1},
                {"color": "yellow", "value": 0},
                {"color": "green", "value": 0.01}
              ]
            },
            "unit": "none"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "values": ["current", "min", "max"]
          }
        }
      },
      {
        "id": 3,
        "title": "⚖️ Weight Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "mc_autotune_weight_latency",
            "legendFormat": "Latency",
            "refId": "A"
          },
          {
            "expr": "mc_autotune_weight_cost",
            "legendFormat": "Cost",
            "refId": "B"
          },
          {
            "expr": "mc_autotune_weight_carbon",
            "legendFormat": "Carbon",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "none",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["percent"]
          }
        }
      },
      {
        "id": 4,
        "title": "🛡️ Correctness Floor Status",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "mc:correctness_floor_breaches_30m",
            "legendFormat": "Breaches (30m)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "red", "value": 0.1}
              ]
            },
            "unit": "none"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 5,
        "title": "💰 Budget Utilization",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "mc:qam_budget_utilization_percent{tenant=~\"$tenant\"}",
            "legendFormat": "{{tenant}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 120
          }
        },
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 6,
        "title": "📊 Performance Regression",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "mc:qam_p95_current",
            "legendFormat": "Current P95",
            "refId": "A"
          },
          {
            "expr": "mc_qam_baseline_p95_seconds",
            "legendFormat": "Baseline P95",
            "refId": "B"
          },
          {
            "expr": "mc_qam_baseline_p95_seconds * 1.05",
            "legendFormat": "Threshold (+5%)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "values": ["current", "mean"]
          }
        }
      },
      {
        "id": 7,
        "title": "🔍 Exploration Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "mc_opt_explore_rate",
            "legendFormat": "Explore Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.02},
                {"color": "green", "value": 0.03},
                {"color": "yellow", "value": 0.07},
                {"color": "red", "value": 0.08}
              ]
            },
            "unit": "none",
            "decimals": 3
          }
        },
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 8,
        "title": "🎯 Tri-Objective Scores",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "mc:tri_opt_performance_score",
            "legendFormat": "Performance Score",
            "refId": "A"
          },
          {
            "expr": "mc:tri_opt_cost_score",
            "legendFormat": "Cost Score",
            "refId": "B"
          },
          {
            "expr": "mc:tri_opt_security_score",
            "legendFormat": "Security Score",
            "refId": "C"
          },
          {
            "expr": "mc:tri_opt_composite_score",
            "legendFormat": "Composite Score",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "none",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "values": ["current", "mean", "max"]
          }
        }
      },
      {
        "id": 9,
        "title": "🔄 Adaptive Learning Progress",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "mc:adaptive_learning_convergence",
            "legendFormat": "Convergence Score",
            "refId": "A"
          },
          {
            "expr": "mc:adaptive_parameter_stability",
            "legendFormat": "Parameter Stability",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "none",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 10,
        "title": "📐 Pareto Frontier Quality",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "mc:pareto_frontier_diversity",
            "legendFormat": "Diversity",
            "refId": "A"
          },
          {
            "expr": "mc:pareto_frontier_coverage",
            "legendFormat": "Coverage",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            },
            "unit": "none",
            "decimals": 2
          }
        },
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 11,
        "title": "⚡ Resource Allocation Efficiency",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
        "targets": [
          {
            "expr": "mc:resource_allocation_efficiency",
            "legendFormat": "Allocation Efficiency",
            "refId": "A"
          },
          {
            "expr": "mc:quantum_backend_utilization",
            "legendFormat": "Backend Utilization",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "none"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 12,
        "title": "🏥 System Health Score",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
        "targets": [
          {
            "expr": "mc:canary_gate_health_score",
            "legendFormat": "Health Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            },
            "unit": "none",
            "min": 0,
            "max": 1,
            "decimals": 2
          }
        },
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 13,
        "title": "🔐 Privacy Budget (ε) Utilization",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 56},
        "targets": [
          {
            "expr": "mc:opt_epsilon_utilization_percent{tenant=~\"$tenant\"}",
            "legendFormat": "{{tenant}} ε Utilization",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "values": ["current", "max"]
          }
        }
      },
      {
        "id": 14,
        "title": "📋 Recent Gate Events",
        "type": "logs",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 64},
        "targets": [
          {
            "expr": "{job=\"mc-platform\"} |= \"gate\" |= \"FAIL|PASS|WARNING\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "sortOrder": "Descending",
          "wrapLogMessage": true
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Canary Gate Failures",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\"MC_.*\", severity=\"critical\"}",
          "iconColor": "red",
          "tagKeys": "alertname,severity",
          "textFormat": "{{alertname}}: {{description}}",
          "titleFormat": "Gate Failure"
        },
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "mc_deployment_timestamp",
          "iconColor": "blue",
          "tagKeys": "version,environment",
          "textFormat": "Deployed {{version}} to {{environment}}",
          "titleFormat": "Deployment"
        }
      ]
    }
  }
}