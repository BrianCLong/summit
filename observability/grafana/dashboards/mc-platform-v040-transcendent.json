{
  "dashboard": {
    "id": null,
    "title": "MC Platform v0.4.0 - Transcendent Intelligence",
    "tags": ["mc-platform", "v0.4.0", "transcendent", "quantum"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Transcendent Intelligence Overview",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "mc_platform:transcendent_performance_multiplier",
            "legendFormat": "Performance Multiplier",
            "refId": "A"
          },
          {
            "expr": "mc_platform_intelligence_quotient",
            "legendFormat": "Intelligence Quotient",
            "refId": "B"
          },
          {
            "expr": "mc_platform_quantum_advantage_ratio",
            "legendFormat": "Quantum Advantage",
            "refId": "C"
          },
          {
            "expr": "mc_platform_autonomy_tier",
            "legendFormat": "Autonomy Tier",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 5 },
                { "color": "green", "value": 10 }
              ]
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Intelligence Quotient" },
              "properties": [{ "id": "unit", "value": "short" }]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Evolution System Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(mc_platform_evolution_proposals_submitted_total[5m]) * 60",
            "legendFormat": "Proposals/min",
            "refId": "A"
          },
          {
            "expr": "rate(mc_platform_evolution_proposals_applied_total[5m]) * 60",
            "legendFormat": "Applied/min",
            "refId": "B"
          },
          {
            "expr": "mc_platform:evolution_success_rate_5m",
            "legendFormat": "Success Rate %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Rate",
            "min": 0
          },
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ],
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": true,
          "show": true,
          "total": false,
          "values": true
        }
      },
      {
        "id": 3,
        "title": "Quantum Operations Metrics",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "mc_platform_quantum_coherence_time_microseconds",
            "legendFormat": "Coherence Time (Î¼s)",
            "refId": "A"
          },
          {
            "expr": "mc_platform_quantum_entangled_qubits",
            "legendFormat": "Entangled Qubits",
            "refId": "B"
          },
          {
            "expr": "mc_platform_quantum_error_rate * 100",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Value",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 50,
            "colorMode": "critical",
            "op": "lt",
            "yAxis": "left"
          },
          {
            "value": 1000,
            "colorMode": "critical",
            "op": "gt",
            "yAxis": "left"
          }
        ]
      },
      {
        "id": 4,
        "title": "Safety Monitoring",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "mc_platform_safety_score",
            "legendFormat": "Safety Score",
            "refId": "A"
          },
          {
            "expr": "mc_platform_human_oversight_enabled",
            "legendFormat": "Human Oversight",
            "refId": "B"
          },
          {
            "expr": "mc_platform_emergency_rollback_ready",
            "legendFormat": "Rollback Ready",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 0.8 },
                { "color": "green", "value": 0.95 }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 5,
        "title": "Autonomy Monitoring",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 16
        },
        "targets": [
          {
            "expr": "mc_platform_autonomy_compensation_rate_24h * 100",
            "legendFormat": "Compensation Rate %",
            "refId": "A"
          },
          {
            "expr": "mc_platform_autonomy_operation_duration_seconds / 3600",
            "legendFormat": "Operation Duration (h)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Value",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 0.5,
            "colorMode": "critical",
            "op": "gt",
            "yAxis": "left"
          }
        ]
      },
      {
        "id": 6,
        "title": "Evidence & Compliance",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(mc_platform_evidence_generation_total[5m]) * 60",
            "legendFormat": "Evidence Gen/min",
            "refId": "A"
          },
          {
            "expr": "mc_platform_data_residency_compliant",
            "legendFormat": "Data Residency OK",
            "refId": "B"
          },
          {
            "expr": "mc_platform_purpose_limitation_compliant",
            "legendFormat": "Purpose Limit OK",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Sandbox Validation Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        },
        "targets": [
          {
            "expr": "rate(mc_platform_evolution_sandbox_validation_attempts_total[5m]) * 60",
            "legendFormat": "Validation Attempts/min",
            "refId": "A"
          },
          {
            "expr": "(rate(mc_platform_evolution_sandbox_validation_failures_total[5m]) / rate(mc_platform_evolution_sandbox_validation_attempts_total[5m])) * 100",
            "legendFormat": "Failure Rate %",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(mc_platform_sandbox_validation_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Duration",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Rate/Duration",
            "min": 0
          },
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 8,
        "title": "ZK Fairness Proof Status",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        },
        "targets": [
          {
            "expr": "mc_platform_zk_fairness_score",
            "legendFormat": "Fairness Score",
            "refId": "A"
          },
          {
            "expr": "mc_platform_zk_demographic_parity",
            "legendFormat": "Demographic Parity",
            "refId": "B"
          },
          {
            "expr": "mc_platform_zk_equalized_odds",
            "legendFormat": "Equalized Odds",
            "refId": "C"
          },
          {
            "expr": "mc_platform_zk_individual_fairness",
            "legendFormat": "Individual Fairness",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Score",
            "min": 0,
            "max": 1
          }
        ],
        "thresholds": [
          {
            "value": 0.8,
            "colorMode": "critical",
            "op": "lt",
            "yAxis": "left"
          }
        ]
      },
      {
        "id": 9,
        "title": "Recent Alerts",
        "type": "logs",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        },
        "targets": [
          {
            "expr": "{job=\"prometheus\", alertname=~\".*Transcendent.*|.*Evolution.*|.*Quantum.*|.*Autonomy.*\"}",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "sortOrder": "Descending",
          "wrapLogMessage": true
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(mc_platform_safety_score, tenant)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "transcendence_level",
          "type": "query",
          "query": "label_values(mc_platform_transcendence_level, transcendence_level)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Evolution Events",
          "datasource": "Prometheus",
          "expr": "increase(mc_platform_evolution_proposals_applied_total[1m]) > 0",
          "iconColor": "blue",
          "titleFormat": "Evolution Applied",
          "textFormat": "{{ $labels.proposal_id }}"
        },
        {
          "name": "Safety Alerts",
          "datasource": "Prometheus",
          "expr": "ALERTS{alertname=~\".*Safety.*|.*Emergency.*\"}",
          "iconColor": "red",
          "titleFormat": "{{ $labels.alertname }}",
          "textFormat": "{{ $labels.summary }}"
        }
      ]
    }
  }
}
