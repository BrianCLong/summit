{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Current SLO compliance for all services",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "max": 100,
          "min": 99,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 99.5 },
              { "color": "green", "value": 99.9 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.4.7",
      "targets": [
        {
          "expr": "(1 - (sum(rate(http_requests_total{service=\"api\",code=~\"5..\"}[30d])) / sum(rate(http_requests_total{service=\"api\"}[30d])))) * 100",
          "legendFormat": "API",
          "refId": "A"
        },
        {
          "expr": "(1 - (sum(rate(http_requests_total{service=\"gateway\",code=~\"5..\"}[30d])) / sum(rate(http_requests_total{service=\"gateway\"}[30d])))) * 100",
          "legendFormat": "Gateway",
          "refId": "B"
        },
        {
          "expr": "(1 - (sum(rate(http_requests_total{service=\"web\",code=~\"5..\"}[30d])) / sum(rate(http_requests_total{service=\"web\"}[30d])))) * 100",
          "legendFormat": "Web",
          "refId": "C"
        }
      ],
      "title": "Service SLO Compliance (30d)",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Remaining error budget for each service",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "center",
            "cellOptions": { "type": "color-background" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "id": 2,
      "options": {
        "showHeader": true,
        "cellHeight": "sm"
      },
      "pluginVersion": "10.4.7",
      "targets": [
        {
          "expr": "(1 - ((sum(rate(http_requests_total{service=\"api\",code=~\"5..\"}[30d])) / sum(rate(http_requests_total{service=\"api\"}[30d]))) / 0.001)) * 100",
          "format": "table",
          "instant": true,
          "legendFormat": "API",
          "refId": "A"
        }
      ],
      "title": "Error Budget Remaining",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": { "Value": "Budget %" }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Rate at which error budget is being consumed",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisLabel": "Burn Rate",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "orange", "value": 6 },
              { "color": "red", "value": 14.4 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "id": 3,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{service=\"api\",code=~\"5..\"}[1h])) / sum(rate(http_requests_total{service=\"api\"}[1h]))) / 0.001",
          "legendFormat": "API Burn Rate (1h)",
          "refId": "A"
        }
      ],
      "title": "Error Budget Burn Rate",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Percentage of cross-tenant attempts successfully denied (target 99.99%+).",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 99.9 },
              { "color": "green", "value": 99.99 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "id": 4,
      "options": {
        "legend": {
          "calcs": ["lastNotNull"],
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "(sum(rate(authz_cross_tenant_decisions_total{decision=\"deny\"}[5m])) / sum(rate(authz_cross_tenant_decisions_total[5m]))) * 100",
          "legendFormat": "Isolation success",
          "refId": "A"
        }
      ],
      "title": "Cross-Tenant Isolation SLI",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "p95 latency for tenant-scoped API requests.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisLabel": "seconds",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.3 },
              { "color": "red", "value": 0.4 }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "id": 5,
      "options": {
        "legend": {
          "calcs": ["lastNotNull"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(companyos_tenant_api_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "Tenant API p95",
          "refId": "A"
        }
      ],
      "title": "Tenant API Latency (p95)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Metering pipeline lag p95, highlighting freshness risk.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisLabel": "seconds",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 120 }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "id": 6,
      "options": {
        "legend": {
          "calcs": ["lastNotNull", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(metering_lag_seconds_bucket[5m])) by (le))",
          "legendFormat": "Metering lag p95",
          "refId": "A"
        }
      ],
      "title": "Metering Lag (p95)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Authentication error rate over 5m windows, targeting <0.05%.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "max": 5,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.03 },
              { "color": "red", "value": 0.05 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "id": 7,
      "options": {
        "legend": {
          "calcs": ["lastNotNull"],
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "(sum(rate(auth_requests_total{status=\"error\"}[5m])) / sum(rate(auth_requests_total[5m]))) * 100",
          "legendFormat": "Auth error rate",
          "refId": "A"
        }
      ],
      "title": "Authentication Error Rate",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["summit", "slo", "sre"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timezone": "",
  "title": "Summit - SLO Overview",
  "uid": "summit-slo-overview",
  "version": 1
}
