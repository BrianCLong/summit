    {
      "title": "IntelGraph Service: Request Rate",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(intelgraph_service_operations_total[5m])) by (method)",
          "legendFormat": "{{method}}"
        }
      ]
    },
    {
      "title": "IntelGraph Service: Error Rate",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(intelgraph_service_operations_total{status='error'}[5m])) by (method) / sum(rate(intelgraph_service_operations_total[5m])) by (method)",
          "legendFormat": "{{method}}"
        }
      ]
    },
    {
      "title": "IntelGraph Service: p99 Request Latency",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(intelgraph_service_request_latency_seconds_bucket[5m])) by (le, method))",
          "legendFormat": "{{method}}"
        }
      ],
      "unit": "s"
    }
