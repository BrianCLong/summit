{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "summit.io.evidence.inauth_metrics",
  "type": "object",
  "required": ["evidence_id", "metrics"],
  "properties": {
    "evidence_id": {"type": "string", "pattern": "^EVD-[A-Z0-9-]+$"},
    "metrics": {
      "type": "object",
      "required": ["provider", "total_profiles", "inauthentic_profiles", "pct"],
      "properties": {
        "provider": {"type": "string"},
        "total_profiles": {"type": "integer", "minimum": 0},
        "inauthentic_profiles": {"type": "integer", "minimum": 0},
        "pct": {"type": "number", "minimum": 0, "maximum": 100},
        "window_start": {"type": "string", "format": "date-time"},
        "window_end": {"type": "string", "format": "date-time"},
        "reach_estimate": {"type": "integer"}
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}