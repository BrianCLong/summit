{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["timestamp", "git_sha"],
  "properties": {
    "timestamp": { "type": "string", "format": "date-time" },
    "git_sha": { "type": "string", "pattern": "^[a-f0-9]{40}$" }
  },
  "additionalProperties": false
}
