{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Summit Evidence Stamp",
  "type": "object",
  "required": ["evidence_id", "generated_at"],
  "properties": {
    "evidence_id": { "type": "string" },
    "generated_at": { "type": "string", "description": "RFC3339 timestamp (timestamps allowed only in stamp.json)" },
    "run_id": { "type": "string" },
    "created_at": { "type": ["string", "null"] }
  },
  "additionalProperties": false
}
