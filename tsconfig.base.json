{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "noEmit": true
  },
  "exclude": [
    "**/*.updated.ts",
    "**/*.updated.tsx",
    "archive_*/**",
    "out/**",
    "ui/build/**"
  ]
}
