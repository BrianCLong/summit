{
  "iamRoles": [
    {
      "name": "pilot-ci",
      "statements": [
        {
          "action": "sts:AssumeRoleWithWebIdentity",
          "resource": "arn:aws:iam::111111111111:role/pilot-ci",
          "effect": "allow"
        },
        {
          "action": "s3:GetObject",
          "resource": "arn:aws:s3:::pilot-artifacts/*",
          "effect": "allow"
        }
      ]
    }
  ],
  "exposures": {
    "publicEndpoints": []
  }
}
