{
  "schemaVersion": 37,
  "title": "Ops Guardian SLOs",
  "panels": [
    {
      "type": "stat",
      "title": "prov-ledger RPS",
      "targets": [{ "expr": "sum(rate(http_requests_total{service=\"prov-ledger\"}[5m]))" }]
    },
    {
      "type": "graph",
      "title": "p95 latency",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))"
        }
      ]
    }
  ]
}
