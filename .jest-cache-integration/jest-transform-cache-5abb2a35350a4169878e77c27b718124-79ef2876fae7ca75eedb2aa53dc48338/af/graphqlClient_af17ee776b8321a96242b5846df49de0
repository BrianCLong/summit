const { http } = require('./httpClient');
function createGraphQLClient(app) {
    const client = http(app);
    return {
        query: async function (options = {}) {
            const { query = '', variables } = options;
            const res = await client.post('/graphql').send({ query, variables });
            return {
                status: res.status,
                body: res.body,
                headers: res.headers,
            };
        },
        mutate: async function (options = {}) {
            const { query = '', variables } = options;
            const res = await client.post('/graphql').send({ query, variables });
            return {
                status: res.status,
                body: res.body,
                headers: res.headers,
            };
        },
        close: async function () {
            return undefined;
        },
    };
}
// For ES module compatibility as well
module.exports = { createGraphQLClient };
exports = module.exports;
