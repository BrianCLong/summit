[
  {
    "name": "AWS Access Key ID",
    "pattern": "\\bAKIA[0-9A-Z]{16}\\b"
  },
  {
    "name": "GitHub Token",
    "pattern": "\\bgh[pousr]_[A-Za-z0-9_]{36,}\\b"
  },
  {
    "name": "Slack Bot Token",
    "pattern": "\\bxoxb-[0-9]{12}-[0-9]{12}-[A-Za-z0-9]{24}\\b"
  },
  {
    "name": "Bearer Token",
    "pattern": "\\bBearer\\s+[A-Za-z0-9\\-\\._~\\+\\/]+=*"
  }
]
