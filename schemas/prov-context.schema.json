{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "@context": {
      "type": "object",
      "properties": {
        "@version": { "type": "number", "const": 1.1 },
        "prov": { "type": "string", "format": "uri" },
        "ol": { "type": "string", "format": "uri" },
        "otel": { "type": "string", "format": "uri" }
      },
      "required": ["@version", "prov", "ol", "otel"]
    }
  },
  "required": ["@context"]
}
