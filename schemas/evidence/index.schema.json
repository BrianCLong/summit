{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["evidence"],
  "properties": {
    "version": { "type": "string" },
    "evidence": {
      "type": "object",
      "patternProperties": {
        "^EVD-[A-Z0-9-]+$": { "type": "string" }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
