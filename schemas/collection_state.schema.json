{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OSINT Collection State",
  "type": "object",
  "properties": {
    "sessionId": { "type": "string" },
    "timestamp": { "type": "string", "format": "date-time" },
    "sessionState": {
      "type": "object",
      "properties": {
        "headers": { "type": "object" },
        "cookies": { "type": "array" },
        "viewport": { "type": "string" }
      }
    },
    "frictionEvents": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": { "type": "string", "enum": ["CAPTCHA", "403", "BLOCKED", "TIMEOUT"] },
          "timestamp": { "type": "string", "format": "date-time" },
          "details": { "type": "string" }
        }
      }
    },
    "accessSequence": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "required": ["sessionId", "timestamp", "sessionState"]
}
