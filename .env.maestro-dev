# Maestro Development Environment Configuration
NAMESPACE=maestro-system

# Database Connections
NEO4J_URI=bolt://neo4j.data.svc.cluster.local:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=YOUR_NEO4J_PASSWORD_HERE

POSTGRES_URI=postgresql://maestro_user:YOUR_POSTGRES_PASSWORD@postgres.data.svc.cluster.local:5432/intelgraph

# Cache & Session Store
REDIS_URL=redis://redis.data.svc.cluster.local:6379

# Authentication & Security
JWT_ISSUER=https://auth.dev.intelgraph.ai
JWT_AUDIENCE=intelgraph
JWT_PUBLIC_KEY=-----BEGIN PUBLIC KEY-----
YOUR_JWT_PUBLIC_KEY_HERE
-----END PUBLIC KEY-----

# AI/ML Endpoints
LLM_LIGHT_BASE_URL=https://api.openai.com/v1
LLM_LIGHT_API_KEY=sk-YOUR_OPENAI_API_KEY_HERE
LLM_HEAVY_BASE_URL=https://api.anthropic.com/v1
LLM_HEAVY_API_KEY=sk-ant-YOUR_ANTHROPIC_KEY_HERE

# Resource Limits
CONDUCTOR_BUDGET_DAILY_USD=100.00
CONDUCTOR_RPS_MAX=1000

# API Configuration
ENABLE_API_KEYS=1
API_KEYS=dev-admin-key:admin:24,dev-read-key:read:24

# Monitoring & Alerting
PAGERDUTY_ROUTING_KEY=YOUR_PAGERDUTY_ROUTING_KEY_HERE

# Maestro Configuration
MAESTRO_HOST=maestro.intelgraph.ai

# V24 Global Coherence Ecosystem Config
COLLABORATION_MODE=true
ENGAGEMENT_INTENSITY=1.0
GLOBAL_IMPACT=1e15
INTEGRITY_THRESHOLD=0.0000000001
COMPLIANCE_STANDARD=true
OPPORTUNITY_PRECISION=0.0000000001
STABILIZATION_NEXUS=1.0
COHERENCE_SCALE=10000000000000

# Environment
NODE_ENV=development
LOG_LEVEL=debug
PORT=4000