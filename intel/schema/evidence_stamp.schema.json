{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Intel Evidence Stamp",
  "type": "object",
  "required": ["evidence_id", "created_at", "git_commit", "run_id"],
  "properties": {
    "evidence_id": {
      "type": "string",
      "pattern": "^EVD-[A-Z0-9\\-]+$"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "git_commit": {
      "type": "string"
    },
    "run_id": {
      "type": "string"
    },
    "version": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
