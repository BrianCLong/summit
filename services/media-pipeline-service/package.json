{
  "name": "@intelgraph/media-pipeline-service",
  "version": "1.0.0",
  "description": "Media and communications pipeline service for intake and processing of audio, video, chat logs, and documents",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=__tests__",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=tests/integration",
    "test:load": "tsx tests/load/load-test.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx src/scripts/migrate.ts"
  },
  "dependencies": {
    "fastify": "^5.6.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.0.4",
    "pg": "^8.11.0",
    "ioredis": "^5.3.2",
    "zod": "^4.1.12",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "uuid": "^13.0.0",
    "bullmq": "^5.34.8",
    "axios": "^1.6.0",
    "mime-types": "^2.1.35",
    "file-type": "^19.0.0",
    "franc": "^6.2.0",
    "iso-639-1": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/uuid": "^10.0.0",
    "@types/mime-types": "^2.1.4",
    "typescript": "^5.9.3",
    "tsx": "^4.7.0",
    "jest": "^30.2.0",
    "@types/jest": "^30.0.0",
    "ts-jest": "^29.1.0",
    "eslint": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  }
}
