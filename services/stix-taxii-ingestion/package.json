{
  "name": "@intelgraph/stix-taxii-ingestion",
  "version": "1.0.0",
  "description": "STIX 2.1/TAXII 2.1 feed ingestion service with pgvector and Neo4j storage",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:benchmark": "vitest bench",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "neo4j-driver": "^5.15.0",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "pino": "^8.17.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "openai": "^4.20.0",
    "@opentelemetry/api": "^1.7.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "https-proxy-agent": "^7.0.2",
    "socks-proxy-agent": "^8.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "stix",
    "taxii",
    "threat-intelligence",
    "ioc",
    "cyber-security",
    "pgvector",
    "neo4j"
  ],
  "author": "IntelGraph Team",
  "license": "PROPRIETARY"
}
