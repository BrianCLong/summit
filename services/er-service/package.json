{
  "name": "@intelgraph/er-service",
  "version": "1.0.0",
  "description": "Entity Resolution & Identity Graph Service for IntelGraph Platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix"
  },
  "dependencies": {
    "@intelgraph/canonical-entities": "workspace:*",
    "@intelgraph/identity-resolution": "workspace:*",
    "@intelgraph/common-types": "workspace:*",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "ioredis": "^5.8.2",
    "kafkajs": "^2.2.4",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "natural": "^8.1.0",
    "fuse.js": "^7.1.0",
    "prom-client": "^15.1.3",
    "@opentelemetry/api": "^1.9.0",
    "rxjs": "^7.8.1",
    "bullmq": "^5.63.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^11.0.0",
    "@jest/globals": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}
