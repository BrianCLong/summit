{
  "name": "@intelgraph/gov-ai-governance",
  "version": "2.0.0",
  "description": "Open source, auditable AI governance for government services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "government",
    "ai-governance",
    "transparency",
    "ethics",
    "compliance",
    "audit",
    "privacy",
    "gdpr",
    "nist",
    "opa"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "express": "^4.18.2",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "supertest": "^6.3.3",
    "tsx": "^4.6.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src/opa-policies"
