{
  "name": "@intelgraph/sandbox-tenant-profile",
  "version": "1.0.0",
  "description": "Sandbox tenant profile management for research and data lab environments",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.23.8",
    "uuid": "^9.0.0",
    "pino": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@intelgraph/innovation-sandbox": "workspace:*"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "sandbox",
    "tenant",
    "data-lab",
    "research",
    "isolation"
  ],
  "author": "IntelGraph Team",
  "license": "MIT"
}
