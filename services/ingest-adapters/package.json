{
  "name": "@intelgraph/ingest-adapters",
  "version": "4.2.3",
  "description": "Ingest adapters for S3/GCS, webhooks, SFTP, and Kafka sources with backpressure and replay support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*.d.ts",
      "import": "./dist/adapters/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-sqs": "^3.450.0",
    "@google-cloud/storage": "^7.7.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "express": "^5.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "p-limit": "^5.0.0",
    "papaparse": "^5.4.1",
    "parquetjs-lite": "^0.8.7",
    "pino": "^8.16.2",
    "pino-http": "^11.0.0",
    "ssh2-sftp-client": "^9.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/papaparse": "^5.3.14",
    "@types/ssh2-sftp-client": "^9.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "keywords": [
    "ingest",
    "etl",
    "adapters",
    "s3",
    "gcs",
    "kafka",
    "sftp",
    "webhook",
    "backpressure"
  ]
}
