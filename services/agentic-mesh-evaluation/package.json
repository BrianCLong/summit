{
  "name": "@summit/agentic-mesh-evaluation",
  "version": "1.0.0",
  "description": "Comprehensive Agentic Mesh Evaluation System - Distributed AI Agent Coordination & Performance Analysis",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "test:e2e": "NODE_ENV=test jest --testPathPattern=e2e",
    "test:load": "k6 run tests/load/mesh-load-test.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "node scripts/migrate.js",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "agentic-mesh",
    "multi-agent-system",
    "distributed-ai",
    "evaluation",
    "coordination",
    "performance",
    "mesh-network",
    "agent-collaboration",
    "mil-spec",
    "enterprise"
  ],
  "author": "Summit Platform",
  "license": "MIT",
  "dependencies": {
    "fastify": "^5.6.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
