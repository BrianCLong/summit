    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.60.0",
    "@opentelemetry/instrumentation": "^0.60.0",
    "@opentelemetry/exporter-prometheus": "^0.60.0",
    "@opentelemetry/exporter-jaeger": "^1.30.0",
    "bcrypt": "^5.1.1",
