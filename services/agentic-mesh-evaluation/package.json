    "@fastify/multipart": "^9.0.1",
    "@fastify/static": "^8.0.2",
    "graphql": "^16.9.0",
    "mercurius": "^16.3.0",
    "ioredis": "^5.8.2",
    "bull": "^4.12.0",
    "pg": "^8.11.3",
    "neo4j-driver": "^5.17.0",
