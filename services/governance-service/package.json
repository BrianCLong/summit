{
  "name": "@summit/governance-service",
  "version": "1.0.0",
  "description": "REST API service for data governance operations including policy management, compliance reporting, and privacy requests",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@summit/data-governance": "workspace:*",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "swagger-ui-express": "^5.0.1",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.18.3",
    "pg": "^8.11.0",
    "dotenv": "^16.4.7",
    "uuid": "^13.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/uuid": "^11.0.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "typescript": "^5.9.3",
    "tsx": "^4.7.0",
    "jest": "^30.2.0",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4"
  },
  "types": "dist/server.d.ts",
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
