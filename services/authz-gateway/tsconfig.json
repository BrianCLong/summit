{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowImportingTsExtensions": false,
    "composite": true,
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "types": []
  },
  "include": [
    "src/**/*",
    "src/**/*.json"
  ]
}
