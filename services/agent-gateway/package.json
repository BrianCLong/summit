{
  "name": "@summit/agent-gateway",
  "version": "1.0.0",
  "description": "Agent Gateway Service for Summit - Agentic Automation Framework",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:safety": "NODE_ENV=test jest --testPathPattern=safety-scenarios",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": [
    "agent",
    "ai",
    "automation",
    "gateway",
    "summit",
    "governance",
    "safety"
  ],
  "author": "Summit Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "bcrypt": "^5.1.1",
    "node-fetch": "^3.3.2",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.11",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
