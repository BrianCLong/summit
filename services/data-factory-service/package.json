{
  "name": "@intelgraph/data-factory-service",
  "version": "1.0.0",
  "description": "Data Factory service for dataset curation, labeling workflows, and training data management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --config jest.config.cjs",
    "test:unit": "jest --config jest.config.cjs --testPathPattern=test/unit",
    "test:e2e": "jest --config jest.config.cjs --testPathPattern=test/e2e",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "fastify": "^4.26.2",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "parquetjs": "^0.11.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.3.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrianCLong/summit.git",
    "directory": "services/data-factory-service"
  },
  "keywords": [
    "data-factory",
    "labeling",
    "ml-training",
    "datasets",
    "intelgraph"
  ],
  "license": "UNLICENSED"
}
