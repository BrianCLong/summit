{
  "name": "@intelgraph/scenario-registry",
  "version": "0.1.0",
  "description": "Scenario registry service for Agentic Mesh evaluation",
  "type": "module",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -b",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:migrate": "node dist/db/migrate.js"
  },
  "keywords": [
    "scenario-registry",
    "evaluation",
    "agentic-mesh"
  ],
  "author": "Summit Platform Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@intelgraph/mesh-eval-sdk": "workspace:*",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "zod": "^3.22.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
