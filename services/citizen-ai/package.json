{
  "name": "@intelgraph/citizen-ai",
  "version": "2.0.0",
  "description": "Multi-lingual AI services for Estonian public services - translation, NLU, and conversational AI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@intelgraph/language-models": "workspace:*",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "nlu",
    "translation",
    "conversational-ai",
    "multilingual",
    "public-services",
    "estonia",
    "e-residency"
  ],
  "author": "IntelGraph",
  "license": "MIT"
}
