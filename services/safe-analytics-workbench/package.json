{
  "name": "@intelgraph/safe-analytics-workbench",
  "version": "0.1.0",
  "description": "Safe Analytics Workbench - Governed environments for data exploration and analysis",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "analytics",
    "workbench",
    "sandbox",
    "data-governance",
    "compliance"
  ],
  "author": "IntelGraph Platform Team",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.18.2",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
