{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Switchboard",
  "version": "0.1.0",
  "identifier": "com.companyos.switchboard",
  "build": {
    "beforeDevCommand": "npm --prefix ../apps/web run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "npm --prefix ../apps/web run build"
  },
  "app": {
    "windows": [
      {
        "title": "CompanyOS Switchboard",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": false
  }
}
