{
  "dashboard": {
    "id": null,
    "title": "Maestro Orchestration System",
    "tags": ["maestro", "orchestration", "ai"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(maestro_orchestration_requests_total, instance)",
          "refresh": "time",
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Orchestration Requests",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(maestro_orchestration_requests_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [100],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "model": {
                  "expr": "rate(maestro_orchestration_requests_total[5m])",
                  "refId": "A"
                },
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "30s",
          "handler": 1,
          "name": "High Request Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "title": "Thompson Sampling Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "maestro_thompson_sampling_reward_rate",
            "legendFormat": "Reward Rate - {{model}}",
            "refId": "A"
          },
          {
            "expr": "maestro_thompson_sampling_exploration_rate",
            "legendFormat": "Exploration Rate",
            "refId": "B"
          }
        ]
      },
      {
        "id": 3,
        "title": "Premium Routing Decisions",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(maestro_premium_routing_decisions_total[5m])",
            "legendFormat": "{{decision}} - {{model_tier}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Web Scraping Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "rate(maestro_web_scraping_requests_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Compliance Gate Results",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "targets": [
          {
            "expr": "rate(maestro_compliance_gate_decisions_total[5m])",
            "legendFormat": "{{decision}} - {{policy}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Orchestration Latency Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(maestro_orchestration_duration_seconds_bucket{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "yAxis": {
          "unit": "s"
        },
        "dataFormat": "tsbuckets"
      },
      {
        "id": 7,
        "title": "Source Synthesis Strategy",
        "type": "pie",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum by (strategy) (maestro_synthesis_strategy_usage_total)",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "AI Model Performance",
        "type": "table",
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 24},
        "targets": [
          {
            "expr": "avg by (model) (maestro_ai_model_response_time_seconds)",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "rate(maestro_ai_model_requests_total[5m])",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "rate(maestro_ai_model_errors_total[5m])",
            "format": "table",
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "Value #A": "Avg Response Time (s)",
                "Value #B": "Request Rate",
                "Value #C": "Error Rate",
                "model": "AI Model"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"maestro-orchestrator\"} / 1024 / 1024",
            "legendFormat": "RSS Memory (MB) - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "go_memstats_heap_inuse_bytes{job=\"maestro-orchestrator\"} / 1024 / 1024",
            "legendFormat": "Heap Memory (MB) - {{instance}}",
            "refId": "B"
          }
        ]
      },
      {
        "id": 10,
        "title": "Goroutines & GC",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "go_goroutines{job=\"maestro-orchestrator\"}",
            "legendFormat": "Goroutines - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "rate(go_gc_duration_seconds_count{job=\"maestro-orchestrator\"}[5m])",
            "legendFormat": "GC Rate - {{instance}}",
            "refId": "B"
          }
        ]
      }
    ]
  }
}