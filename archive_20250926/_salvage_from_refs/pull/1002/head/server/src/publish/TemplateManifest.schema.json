{
  "$id": "https://intelgraph/schema/dp-template-manifest.v1",
  "type": "object",
  "required": ["id", "name", "version", "dp", "kMin", "epsilonTiers", "region", "publisher"],
  "properties": {
    "id": {"type": "string"},
    "name": {"type": "string"},
    "version": {"type": "string"},
    "dp": {"type": "boolean", "const": true},
    "kMin": {"type": "integer", "minimum": 25},
    "epsilonTiers": {"type": "array", "items": {"type": "number", "minimum": 0.01}},
    "region": {"type": "array", "items": {"type": "string"}},
    "publisher": {"type": "string"},
    "entry": {"type": "string"},
    "inputs": {"type": "array", "items": {"type": "string"}}
  }
}
