{
  "name": "doc_graph_copilot",
  "nodes": [
    {"id": "classify", "type": "classifier", "outputs": ["intent"]},
    {"id": "retrieve", "type": "rag", "inputs": ["question"], "outputs": ["context"]},
    {"id": "model", "type": "model", "inputs": ["question", "context", "policy"], "outputs": ["answer"]},
    {"id": "policy", "type": "policy", "inputs": ["answer"], "outputs": ["decision"]}
