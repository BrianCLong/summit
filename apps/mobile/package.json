{
  "name": "@intelgraph/mobile",
  "version": "1.0.0",
  "description": "React Native OSINT mobile app for Summit IntelGraph with offline-first architecture and real-time GraphQL subscriptions",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "pod-install": "cd ios && bundle exec pod install",
    "clean": "watchman watch-del-all && rm -rf node_modules && pnpm install",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "ios:release": "cd ios && xcodebuild -workspace IntelGraphMobile.xcworkspace -scheme IntelGraphMobile -configuration Release",
    "prebuild:ios": "npx expo prebuild -p ios",
    "prebuild:android": "npx expo prebuild -p android",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "update:ota": "eas update",
    "codegen": "graphql-codegen --config codegen.ts",
    "e2e:android": "detox test -c android.emu.release",
    "e2e:ios": "detox test -c ios.sim.release",
    "e2e:build:android": "detox build -c android.emu.release",
    "e2e:build:ios": "detox build -c ios.sim.release"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-native": "^0.76.6",
    "@react-navigation/native": "^7.0.11",
    "@react-navigation/native-stack": "^7.2.2",
    "@react-navigation/bottom-tabs": "^7.2.2",
    "@react-navigation/drawer": "^7.2.2",
    "react-native-screens": "^4.4.0",
    "react-native-safe-area-context": "^5.1.7",
    "react-native-gesture-handler": "^2.21.0",
    "react-native-reanimated": "^4.0.3",
    "nativewind": "^4.1.23",
    "tailwindcss": "^3.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "react-native-mmkv": "^3.1.0",
    "@op-engineering/op-sqlite": "^11.0.0",
    "@nozbe/watermelondb": "^0.27.1",
    "@apollo/client": "^4.0.9",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/query-async-storage-persister": "^5.8.4",
    "@tanstack/react-query-persist-client": "^5.8.4",
    "zustand": "^5.0.8",
    "immer": "^10.2.0",
    "zod": "^4.1.12",
    "react-hook-form": "^7.54.2",
    "@hookform/resolvers": "^5.2.2",
    "react-native-maps": "^1.21.0",
    "@rnmapbox/maps": "^10.1.30",
    "react-native-geolocation-service": "^5.3.1",
    "@turf/turf": "^7.2.0",
    "react-native-clusterer": "^1.2.2",
    "react-native-svg": "^15.15.0",
    "react-native-svg-charts": "^5.4.0",
    "d3": "^7.9.0",
    "@notifee/react-native": "^9.1.8",
