{
  "name": "webapp",
  "version": "4.2.3",
  "description": "Analyst tri-pane web app",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --no-eslintrc --config .eslintrc.cjs \"src/**/*.{ts,tsx}\"",
    "format": "prettier --check 'src/**/*.{ts,tsx,css,html}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:visual": "playwright test --project=visual",
    "test:cypress": "cypress run --browser chromium"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-zone": "^2.5.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/instrumentation-document-load": "^0.56.0",
    "@opentelemetry/instrumentation-fetch": "^0.211.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.55.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-trace-web": "^2.5.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@reduxjs/toolkit": "^2.8.2",
    "cytoscape": "^3.33.1",
    "mapbox-gl": "^3.14.0",
    "react": "^19.2.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.13.0",
    "socket.io-client": "^4.8.1",
    "vis-data": "^8.0.3",
    "vis-timeline": "^8.3.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^4.0.18",
    "cypress": "^13.15.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "jsdom": "^27.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.3",
    "vitest": "^4.0.18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}
