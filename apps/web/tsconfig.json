{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowImportingTsExtensions": false,
    "typeRoots": ["./node_modules/@types", "../../node_modules/@types"],
    "types": ["node", "jest"]
  },
  "include": ["src", "src/**/*.json"],
  "references": []
}
