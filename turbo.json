{
  "pipeline": {
    "build": { 
      "dependsOn": ["^build"], 
      "outputs": ["dist/**", "build/**", ".next/**", "out/**"] 
    },
    "test": { 
      "dependsOn": ["build"], 
      "outputs": ["coverage/**"] 
    },
    "lint": { 
      "outputs": [] 
    },
    "typecheck": { 
      "outputs": ["tsbuildinfo/**"] 
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "remoteCache": {
    "enabled": true
  }
}