{
  "title": "Price-Aware Orchestration",
  "uid": "pao-main",
  "timezone": "browser",
  "editable": true,
  "panels": [
    {
      "type": "timeseries",
      "title": "Pricing refresh success vs error (rate)",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 7 },
      "targets": [
        { "expr": "sum(rate(pao_pricing_refresh_success_total[5m]))", "legendFormat": "success" },
        { "expr": "sum(rate(pao_pricing_refresh_errors_total[5m]))", "legendFormat": "error" }
      ],
      "options": { "legend": { "showLegend": true } }
    },
    {
      "type": "stat",
      "title": "Last successful refresh",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 7 },
      "targets": [{ "expr": "max(pao_pricing_last_success_timestamp_seconds)" }],
      "fieldConfig": {
        "defaults": { "unit": "dateTimeAsIso" },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      }
    },
    {
      "type": "stat",
      "title": "Unknown poolId selections (5m)",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 7 },
      "targets": [{ "expr": "sum(rate(pao_unknown_pool_selections_total[5m]))" }],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green" }, { "color": "red", "value": 0.1 }]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Pool selection count by poolId",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 0, "y": 7, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (pool_id) (rate(pao_pool_selection_total[5m]))",
          "legendFormat": "{{pool_id}}"
        }
      ],
      "options": { "legend": { "showLegend": true } },
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "type": "timeseries",
      "title": "Reservation actions (reserve/release) success vs error",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 12, "y": 7, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (action) (rate(pao_reservation_actions_total{status=\"success\"}[5m]))",
          "legendFormat": "{{action}} success"
        },
        {
          "expr": "sum by (action) (rate(pao_reservation_actions_total{status=\"error\"}[5m]))",
          "legendFormat": "{{action}} error"
        }
      ],
      "options": { "legend": { "showLegend": true } }
    },
    {
      "type": "gauge",
      "title": "Active reservations",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 0, "y": 14, "w": 6, "h": 7 },
      "targets": [{ "expr": "max(pao_active_reservations)" }],
      "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "timeseries",
      "title": "\"Unknown poolId\" selections (timeseries)",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 6, "y": 14, "w": 9, "h": 7 },
      "targets": [
        { "expr": "sum(rate(pao_unknown_pool_selections_total[5m]))", "legendFormat": "unknown" }
      ],
      "options": { "legend": { "showLegend": false } }
    },
    {
      "type": "timeseries",
      "title": "Selector none/unknown outcomes",
      "datasource": { "type": "prometheus" },
      "gridPos": { "x": 15, "y": 14, "w": 9, "h": 7 },
      "targets": [
        { "expr": "sum(rate(pao_selector_no_eligible_total[5m]))", "legendFormat": "no eligible" },
        {
          "expr": "sum(rate(pao_unknown_pool_selections_total[5m]))",
          "legendFormat": "unknown pool"
        }
      ],
      "options": { "legend": { "showLegend": true } }
    }
  ],
  "time": { "from": "now-6h", "to": "now" },
  "templating": { "list": [] },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Dashboards for price-aware orchestration (pricing refresh, pool selection, capacity reservations). Metric names use PAO-prefixed placeholders; align service emission to match."
}
