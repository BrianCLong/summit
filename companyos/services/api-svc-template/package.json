  },
  "devDependencies": {
    "@types/config": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "supertest": "^7.0.0",
    "tsconfig-paths": "^4.3.2",
    "tsx": "^4.16.5",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
