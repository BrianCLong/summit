{
  "name": "golden-path-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint 'src/**/*.ts' 'scripts/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'scripts/**/*.ts'",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "seed": "node --loader ts-node/esm scripts/seed.ts",
    "reset:db": "node --loader ts-node/esm scripts/reset-db.ts",
    "dev:stack": "docker compose -f docker-compose.dev.yml up --build",
    "docker:build": "docker build -t ghcr.io/companyos/golden-path-service:dev ."
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cls-hooked": "^4.2.2",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "pg": "^8.13.0",
    "pino": "^9.5.0",
    "pino-http": "^10.4.0",
    "prom-client": "^15.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.11.6",
    "@types/pino": "^9.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
