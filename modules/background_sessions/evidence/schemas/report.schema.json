{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Background Sessions Evidence Report",
  "type": "object",
  "required": ["id", "summary"],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^EVD-[A-Z0-9_-]+$"
    },
    "summary": {
      "type": "string"
    }
  },
  "additionalProperties": true
}
